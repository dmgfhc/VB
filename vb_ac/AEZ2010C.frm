Private Sub ss2_KeyUp(KeyCode As Integer, Shift As Integer)

    Dim sTemp_Code As String

    If ss2.MaxRows < 1 Then Exit Sub
    
    If KeyCode = vbKeyReturn Or KeyCode = vbKeyEscape Or KeyCode = 229 Then
        Exit Sub
    End If

    Select Case ss2.ActiveCol
    
        Case 6
        
            If KeyCode = vbKeyF4 Then
            
                Set DD.sPname = Me.ss2
                
                DD.sWitch = "SP"
                DD.sKey = "Z0004"
                DD.rControl.Add Item:=6
                DD.rControl.Add Item:=7
                
                DD.nameType = "2"
                Call Gf_Common_DD(M_CN1, KeyCode)
                
                ss2.Col = 6
                If ss2.Text = "B" Then
                    ss2.Text = ""
                    ss2.Col = 7
                    ss2.Text = ""
                    Exit Sub
                End If
                
            Else
            
                ss2.Col = ss2.ActiveCol
                
                If Len(Trim(ss2.Text)) = ss2.TypeMaxEditLen Then
                
                    If ss2.Text = "B" Or ss2.Text = "b" Then
                        ss2.Text = ""
                        ss2.Col = 7
                        ss2.Text = ""
                        Exit Sub
                    End If
                    
                    ss2.Col = 6
                    sTemp_Code = ss2.Text
                    ss2.Col = 7
                    ss2.Text = Gf_ComnNameFind(M_CN1, "Z0004", Trim(sTemp_Code), 2)
                Else
                    ss2.Col = 7
                    ss2.Text = ""
                End If
            
            End If
            
    End Select
    
End Sub

