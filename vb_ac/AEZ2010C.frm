Private Sub cmd_complete_Click()

    Dim lRow As Integer
    
    If ss2.MaxRows < 1 Then Exit Sub
    
    For lRow = 1 To ss2.MaxRows
        ss2.Row = lRow
        ss2.Col = 6
        
        If ss2.Text <> "C" Then
            ss2.Text = "C"
            ss2.Col = 7
            ss2.Text = "结束"
            ss2.Col = 0
            ss2.Text = "Update"
        End If
        
    Next lRow
    
End Sub

