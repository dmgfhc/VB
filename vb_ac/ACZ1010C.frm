Private Sub cbo_SQL_Name_Click()
    Dim sSql            As String
    Dim SQL             As String
    Dim sName           As String
    Dim iDR             As Long
    Dim iDc             As Long
    Dim iLoc            As Long
    Dim iLoc2           As Long
    Dim iLoc3           As Long
    Dim iDx             As Integer
    Dim iSeq            As Integer
    Dim sTable_Name     As String
    Dim sColum_Name     As String
    Dim sAliasName      As String
    Dim sMessg          As String

    On Error Resume Next
    
    If Trim(cbo_SQL_Name.Text) = "" Then Exit Sub
    
'    If Trim(txt_Cond.Text) <> "" Then
'        sMessg = "表格中还有数据未处理，" + vbCrLf
'        sMessg = sMessg + "放弃并继续吗？"
'
'        If Not Gf_MessConfirm(sMessg, "Q") Then
'            Exit Sub
'        End If
'    End If
               
    sName = Trim(cbo_SQL_Name.Text)
    iSeq = cbo_SQL_Name.ListIndex
    
    Set AdoRs = New ADODB.Recordset
    
    SQL = " SELECT      USER_SQL       " & vbCrLf
    SQL = SQL & "  FROM ZP_USER_SQL    " & vbCrLf
    SQL = SQL & " WHERE EMP_ID   =  '" & sUser_ID & "'" & vbCrLf
    SQL = SQL & "   AND SQL_NAME =  '" & sName & "'" & vbCrLf
    
    AdoRs.Open SQL, M_CN1, adOpenForwardOnly, adLockReadOnly

    If AdoRs.RecordCount = 0 Then txt_Cond.Text = "":    Exit Sub
    
    If Left(sName, 3) = "(D)" Then
        chk_Direct_Flag.Value = ssCBChecked
    End If
    
    If chk_Direct_Flag.Value = -1 Then
        txt_Cond1.Text = Replace(Trim(AdoRs.Fields(0) & ""), "@", "'")
        AdoRs.Close
        Exit Sub
    Else
        txt_Cond.Text = Replace(Trim(AdoRs.Fields(0) & ""), "@", "'")
    End If
    
    AdoRs.Close
    
    Screen.MousePointer = vbHourglass
    
    Call cmd_all_clear_Click
    
    sFrameCond.Visible = False
    Call cmd_Auto_Select_Click
    
    sSql = Replace(UCase(txt_Cond.Text), vbCrLf, "")
    iLoc = InStr(1, sSql, "FROM")
    sSql = UCase(Trim(Mid(sSql, iLoc + 4, Len(sSql))))
          
    Do Until sSql = ""
        iLoc = InStr(1, sSql, "WHERE")
        iLoc2 = InStr(1, sSql, ",")
        
        If iLoc2 = 0 Or iLoc2 > iLoc Then
            iLoc = InStr(1, sSql, " ")
            sTable_Name = Trim(Left(sSql, iLoc))
            sSql = ""
        Else
            sTable_Name = Trim(Mid(sSql, 1, iLoc2 - 2))
            sSql = UCase(Trim(Mid(sSql, iLoc2 + 1, Len(sSql))))
        End If
        
        For iDR = 1 To ss2.MaxRows
            ss2.Row = iDR
            ss2.Col = 2
            If sTable_Name = ss2.Text Then
                Call ss2_DblClick(2, iDR)
                Exit For
            End If
        Next iDR
    Loop
    
    sSql = Replace(UCase(txt_Cond.Text), vbCrLf, "")
    iLoc = InStr(1, sSql, "SELECT")
    sSql = UCase(Trim(Mid(sSql, iLoc + 6, Len(sSql))))
          
    Do Until sSql = ""
        iLoc = InStr(1, sSql, " ")
        iLoc2 = InStr(1, sSql, ".")
        iLoc3 = InStr(1, sSql, "(")
        
        If iLoc > iLoc3 And iLoc3 > 0 Then
            iLoc2 = InStr(iLoc3, sSql, ".")
            sAliasName = Left(sSql, iLoc2 - 1)
            iLoc3 = InStr(iLoc3, sSql, ")")
            sColum_Name = Trim(Mid(sSql, iLoc2 + 1, iLoc3 - iLoc2 - 1))
        Else
            sAliasName = Left(sSql, iLoc2 - 1)
            sColum_Name = Trim(Mid(sSql, iLoc2 + 1, iLoc - iLoc2))
        End If
        
        Select Case sAliasName
            Case "A":  iDx = 1
            Case "B":  iDx = 2
            Case "C":  iDx = 3
            Case "D":  iDx = 4
        End Select
            
        With SSTab(iDx)
            For iDR = 1 To .MaxRows
                .Row = iDR
                .Col = 3
                If sColum_Name = .Text Then
                    .Col = 1:   .Text = 1
                    Exit For
                End If
            Next iDR
        End With
        
        iLoc = InStr(1, sSql, "FROM")
        iLoc2 = InStr(1, sSql, ",")
        
        If iLoc2 = 0 Or iLoc2 > iLoc Then
            sSql = ""
        Else
            sSql = UCase(Trim(Mid(sSql, iLoc2 + 1, Len(sSql))))
        End If
        
    Loop
    
    sSql = UCase(txt_Cond.Text)
    iLoc = InStr(1, sSql, "WHERE")
    txt_Where_Cond.Text = UCase(Trim(Mid(sSql, iLoc + 6, Len(sSql))))
    
    CBO_ITEM.Enabled = True
    CBO_FILTER.Enabled = True
    txtCond1.Enabled = True
    cmd_ADD.Enabled = True
    cmd_AND.Enabled = True
    cmd_OR.Enabled = True
    sFrame.Visible = True
    sFrame.ZOrder (0)
'    cbo_SQL_Name.ListIndex = iSeq
    
    Screen.MousePointer = vbDefault
    
End Sub


Private Sub ss2_DblClick(ByVal Col As Long, ByVal Row As Long)
    Dim sTable          As String
    Dim isel            As Integer
    Dim iDx             As Integer
    Dim iDc             As Integer
    Dim iDR             As Integer
    Dim iSelCnt         As Integer
    Dim sTableName      As String
    Dim sTableType      As String
    Dim sColName        As String
    Dim sAlias          As String

    If Row < 1 Then Exit Sub
    
    ss2.Row = Row
    ss2.Col = 0
    If ss2.Text = "选择" Then
        ss2.Text = ""
        ss2.Col = 1
        For iDx = 1 To 4
            SSTab(iDx).Row = 0
            SSTab(iDx).Col = 2
            If Trim(ss2.Text) = Trim(SSTab(iDx).Text) Then
                iSelCnt = iDx
                Exit For
            End If
        Next iDx
        
        SSTab(iSelCnt).MaxRows = 0
        SSTab(iSelCnt).Row = 0
        SSTab(iSelCnt).Col = 2:     SSTab(iSelCnt).Text = ""
        SSTab(iSelCnt).Col = 3:     SSTab(iSelCnt).Text = ""
        
        If iSelCnt <> iSelectCnt Then
            For iDx = iSelCnt + 1 To 4
                Call ItemBoxCopy(iDx, -1)
            Next iDx
        End If
        iSelectCnt = iSelectCnt - 1
        
        txt_Where_Cond.Text = ""
    ElseIf iSelectCnt = 4 Then
        Exit Sub
    Else
        ss2.Text = "选择"
        
        iSelectCnt = iSelectCnt + 1
        
        Call Table_Column_Edit(Row)
    End If
    
    txt_Table_Name.Text = ""
    isel = 0
    For iDc = 1 To ss2.MaxRows
        ss2.Row = iDc
        ss2.Col = 0
        If ss2.Text = "选择" Then
            isel = isel + 1
            If Trim(txt_Table_Name.Text) <> "" Then
                txt_Table_Name.Text = txt_Table_Name.Text & ", "
            End If
            
            ss2.Col = 2
            txt_Table_Name.Text = txt_Table_Name.Text & Trim(ss2.Text) & " " & Chr(isel + 64)
        End If
    Next iDc
    
    ss2.OperationMode = OperationModeNormal
    
    CBO_ITEM.Clear
    
    For iDc = 1 To 4
        SSTab(iDc).Row = 0
        SSTab(iDc).Col = 3
        If Trim(SSTab(iDc).Text) = "" Then Exit For
        sTableName = Trim(SSTab(iDc).Text)
        
        For iDR = 1 To SSTab(iDc).MaxRows
            SSTab(iDc).Row = iDR
            SSTab(iDc).Col = 4
            sTableType = SSTab(iDc).Text
            
            SSTab(iDc).Col = 2
            sColName = Trim(SSTab(iDc).Text)
            
            SSTab(iDc).Col = 3
            If sColName = "" Then sColName = SSTab(iDc).Text
            CBO_ITEM.AddItem Chr(iDc + 64) & "." & SSTab(iDc).Text & "." & sColName & String(100, " ") & sTableType
                        
            SSTab(iDc).OperationMode = OperationModeNormal
        Next iDR
        
        Call Gp_Sp_BlockLock(SSTab(iDc), 2, SSTab(iDc).MaxCols, 1, SSTab(iDc).MaxRows, True)
    Next iDc
    
