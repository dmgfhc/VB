Private Sub ItemBoxCopy(CB_Name_From As Integer, CopyLoc As Long)
    Dim iDR             As Long
    Dim iDc             As Long
    Dim sTableId        As String
    Dim sTableName      As String
    
    If SSTab(CB_Name_From).MaxRows < 1 Then Exit Sub
    If CB_Name_From = 4 And CopyLoc > 0 Then Exit Sub
    
    SSTab(CB_Name_From + CopyLoc).MaxRows = SSTab(CB_Name_From).MaxRows
    SSTab(CB_Name_From + CopyLoc).MaxCols = SSTab(CB_Name_From).MaxCols
    
    For iDR = 1 To SSTab(CB_Name_From).MaxRows
        SSTab(CB_Name_From + CopyLoc).Row = iDR
        SSTab(CB_Name_From).Row = iDR
        For iDc = 1 To SSTab(CB_Name_From).MaxCols
            SSTab(CB_Name_From + CopyLoc).Col = iDc
            SSTab(CB_Name_From).Col = iDc
            If SSTab(CB_Name_From + CopyLoc).CellType = SS_CELL_TYPE_CHECKBOX Then
                SSTab(CB_Name_From + CopyLoc).Value = SSTab(CB_Name_From).Value
            Else
                SSTab(CB_Name_From + CopyLoc).Text = SSTab(CB_Name_From).Text
            End If
        Next iDc
    Next iDR
    
    SSTab(CB_Name_From).Row = 0
    SSTab(CB_Name_From).Col = 2:   sTableName = SSTab(CB_Name_From).Text
    SSTab(CB_Name_From).Col = 3:   sTableId = SSTab(CB_Name_From).Text
    
    SSTab(CB_Name_From + CopyLoc).Row = 0
    SSTab(CB_Name_From + CopyLoc).Col = 2:  SSTab(CB_Name_From + CopyLoc).Text = sTableName
    SSTab(CB_Name_From + CopyLoc).Col = 3:  SSTab(CB_Name_From + CopyLoc).Text = sTableId
    
    If CopyLoc < 0 Then
        SSTab(CB_Name_From).MaxRows = 0
        SSTab(CB_Name_From).Row = 0
        SSTab(CB_Name_From).Col = 2:   SSTab(CB_Name_From).Text = ""
        SSTab(CB_Name_From).Col = 3:   SSTab(CB_Name_From).Text = ""
    End If
End Sub

Private Sub ItemSelect(sItemName As String)
    Dim IDt             As Long
    Dim iDR             As Long
    Dim iDc             As Long
    Dim sTableName      As String
    Dim sTableType      As String
    Dim sColumnId       As String
    Dim sColumnName     As String
    
    sItemName = ""
    
    For IDt = 1 To 4
        SSTab(IDt).Row = 0
        SSTab(IDt).Col = 3
        If Trim(SSTab(IDt).Text) = "" Then Exit Sub
        sTableName = Right(Trim(SSTab(IDt).Text), 1)
        
        For iDR = 1 To SSTab(IDt).MaxRows
            SSTab(IDt).Row = iDR
            SSTab(IDt).Col = 1
            If SSTab(IDt).Value = 1 Then
                If Trim(sItemName) <> "" Then sItemName = sItemName & ","
                SSTab(IDt).Col = 3
                sColumnId = Trim(SSTab(IDt).Text)
                
                SSTab(IDt).Col = 2
                sColumnName = Trim(SSTab(IDt).Text)
                
                If sColumnName = "" Then
                    sColumnName = sColumnId & "_" & Chr(IDt + 64)
                Else
                    sColumnName = sColumnName & Chr(IDt + 64)
                End If
                Call ItemNameEdit(sColumnName)
                
                
                sItemName = sItemName & sTableName & "." & sColumnId & " " & sColumnName & vbCrLf
            End If
        Next iDR
    Next IDt
End Sub

