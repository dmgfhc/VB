Private Sub txt_Cond_Change()
    Dim iDR     As Long
    Dim SQL     As String
    Dim sText   As String
    Dim iLoc    As Long
    Dim iDc     As Long

    lbl_count.Caption = ""

    On Error Resume Next

    Set AdoRs = New ADODB.Recordset
    
    For iDR = 0 To 9
        cmd_Select(iDR).Visible = False
        cmd_Select(iDR).Enabled = False
    Next iDR
    
'    cbo_SQL_Name.Text = ""
    
    sText = Replace(Trim(txt_Cond.Text), vbCrLf, "")
     
    If sText = "" Then Exit Sub

    If UCase(Left(sText, 1)) <> "S" Then
       Call Gp_MsgBoxDisplay("条件必须输入", "", "错误提示")
       Exit Sub
    End If

    iLoc = InStr(1, UCase(sText), "WHERE")
    If iLoc = 0 Then Exit Sub
    
    iLoc = InStr(1, UCase(sText), "FROM")
    If iLoc = 0 Then Exit Sub
    
    SQL = "SELECT COUNT(*) " & Mid(sText, iLoc - 1, Len(sText))

