        End If
    Else
        txt_User_Cond.Text = Trim(txt_User_Cond.Text)
        If Right(txt_User_Cond.Text, 1) = "," Then
            txt_User_Cond.Text = Left(txt_User_Cond.Text, Len(txt_User_Cond.Text) - 1)
