        sdb_mp_slab_len.Value = Val(AdoRs.Fields(3) & "")
        AdoRs.MoveNext
    Loop
    AdoRs.Close
    
    'Process Error Check
    If adoCmd("arg_e_code") <> "Y" Then
        ret_Result_ErrMsg = adoCmd("arg_e_msg")
        sErrMessg = "Error Mesg : " & ret_Result_ErrMsg
