            iVisible_Cnt = iVisible_Cnt + 1
            
            lbl_mplate(iVisible_Cnt).Visible = True
            lbl_mplate(iVisible_Cnt).BackColor = &HC0C0C0
            lbl_mplate(iVisible_Cnt).ForeColor = &HFF0000
            
