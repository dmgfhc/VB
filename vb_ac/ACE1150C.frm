            ElseIf lbl_mplate(iCount).Tag = "ord6" Then
                lbl_mplate(iVisible_Cnt).Width = (Shape1.Width / lCool_max) * sdb_ord6_len.Value
            ElseIf lbl_mplate(iCount).Tag = "ord7" Then
                lbl_mplate(iVisible_Cnt).Width = (Shape1.Width / lCool_max) * sdb_ord7_len.Value
            ElseIf lbl_mplate(iCount).Tag = "ord8" Then
                lbl_mplate(iVisible_Cnt).Width = (Shape1.Width / lCool_max) * sdb_ord8_len.Value
            ElseIf lbl_mplate(iCount).Tag = "ord9" Then
                lbl_mplate(iVisible_Cnt).Width = (Shape1.Width / lCool_max) * sdb_ord9_len.Value
            Else
                lbl_mplate(iVisible_Cnt).Width = (Shape1.Width / lCool_max) * sdb_ord0_len.Value
            End If
                
            lbl_mplate(iVisible_Cnt).Tag = lbl_mplate(iCount).Tag
            
            If iVisible_Cnt = 1 Then
                lbl_mplate(iVisible_Cnt).Left = Shape1.Left
            Else
