                    sSeq = "0" & iCount
                Else
                    sSeq = Str(iCount)
                End If
                
                'CP_REP_SLAB_D UPDATE  BLOCK_SEQ='01', SEQ      --> LEN = 0
                If lbl_mplate(iCount).Tag = "ord1" Then
                    Call Plate_Seq_Create(iOrd1_Curr_Row, sSeq, "U")
