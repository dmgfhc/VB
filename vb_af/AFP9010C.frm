Public Sub Ladle_Edit()
    Dim AdoRs  As ADODB.Recordset
    Dim sQuery As String



    M_CN1.CursorLocation = adUseServer
    Set AdoRs = New ADODB.Recordset
    'Set adoCmd = New adodb.Command

    'adoCmd.CommandType = adCmdText
    'Set adoCmd.ActiveConnection = M_CN1

    sQuery = "         SELECT DISTINCT(LADLE_NO)"
    sQuery = sQuery & "  FROM NISCO.FP_LADLE "
    sQuery = sQuery & " WHERE SUBSTR(LADLE_NO,1,1)='S'"
    sQuery = sQuery & "   ORDER BY LADLE_NO ASC"

    'Ado Execute
    AdoRs.Open sQuery, M_CN1, adOpenKeyset
    'AdoRs.Open sQuery, M_CN1, adOpenForwardOnly, adLockReadOnly
    cbo_ladle_no.Clear
    cbo_ladle_no.AddItem "ALL"
    If Not AdoRs.BOF And Not AdoRs.EOF Then
        While Not AdoRs.EOF

            If VarType(AdoRs.Fields(0)) <> vbNull Then
                  cbo_ladle_no.AddItem AdoRs.Fields(0)
            End If
            AdoRs.MoveNext
        Wend
    End If

    AdoRs.Close
    Set AdoRs = Nothing


    'Call Gf_HeatNo_ComboAdd(M_CN1, cbo_HEAT_NO, "FP_LFRSLT", "DEP_STEEL_WGT", Trim(cbo_prc_line.Text))
    If cbo_ladle_no.ListCount <> 0 And Trim(cbo_ladle_no.Text) = "" Then
       cbo_ladle_no.ListIndex = 0
    End If
    
End Sub


