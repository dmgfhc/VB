Public Sub Form_Ref()

On Error GoTo Refer_Err

    Dim sMesg As String
    Dim A, B As Single
    Dim i, j As Integer
    
'    If Gf_Sp_ProceExist(Proc_Sc("Sc").Item("Spread")) Then Exit Sub
    
    If Gf_Sp_Refer(M_CN1, Proc_Sc("Sc"), Mc1, Mc1("nControl"), Mc1("mControl")) Then
        
        Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
        MDIMain.MenuTool.Buttons(7).Enabled = False
        txt_MAT_MONTH.Enabled = False
        cbo_MAT_KIND.Enabled = False
        
        With ss1

          For i = 1 To .MaxRows
             .Col = 10
             .Row = i
             If .Text = "1" Then
                .Col = 7
                .Lock = True
                .Col = 9
                .Lock = True
             End If
          Next

          For i = 5 To 8
             A = 0
             .Col = i
             For j = 1 To .MaxRows
                .Row = j
                 If .Col = 7 Then
                     If .Text <> "" Then
                         A = A + .Text
                     Else
                         A = A + Val(.Text)
                     End If
                 ElseIf .Col = 5 Or .Col = 6 Or .Col = 8 Then
                     A = A + Val(.Text)
                 End If
             Next

            Select Case .Col
                Case 5
                    SSP1.Caption = A
                Case 6
                    SSP2.Caption = A
                Case 7
                    SSP3.Caption = A
                Case 8
                    SSP4.Caption = A
            End Select
          Next
                            
        End With

        Exit Sub
    End If
            
    Exit Sub

Refer_Err:

End Sub

