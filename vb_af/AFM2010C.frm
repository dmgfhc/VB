Public Sub Form_Ref()
        
    If Gf_Ms_Refer(M_CN1, Mc1, Mc1("pControl"), Mc1("mControl")) Then
        Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
        Call Gp_Ms_ControlLock(Mc1("pControl"), True)
    End If
    
    cbo_sit1_yn = ""
    cbo_sit2_yn = ""
    cbo_sit3_yn = ""
    cbo_area1_yn = ""
    cbo_area2_yn = ""
    cbo_big_all_yn = ""
    
End Sub

Public Sub Form_Pro()

    If Chk_ss6.VALUE = -1 Then
       If Trim(txt_LD_REP_DEPT_DATE.Text) = "____-__-__ __:__:__" Then
          MsgBox "离开时间必须输入！", vbCritical, "系统提示信息"
          Exit Sub
       End If
    End If
    
    If Chk_ss2.VALUE = -1 Then
       If Trim(txt_LD_REP_ARRV_DATE) = "____-__-__ __:__:__" Then
          MsgBox "到达时间必须输入！", vbCritical, "系统提示信息"
          Exit Sub
       End If
    End If
    
    If Chk_ss6.VALUE = -1 Then
       If Trim(cbo_CONVERTER_ID.Text) = "" Then
          MsgBox "炉座号必须输入！", vbCritical, "系统提示信息"
          Exit Sub
       End If
       If Trim(txt_ld_status.Text) = "" Then
          MsgBox "包号状态必须输入！", vbCritical, "系统提示信息"
          Exit Sub
       End If
    End If
    
    If Gf_Mc_Authority(sAuthority, Mc1) Then
        If Gf_Ms_Process(M_CN1, Mc1, sAuthority) Then Call MDIMain.FormMenuSetting(Me, FormType, "SE", sAuthority)
    End If
    
    cbo_sit1_yn = ""
    cbo_sit2_yn = ""
    cbo_sit3_yn = ""
    cbo_area1_yn = ""
    cbo_area2_yn = ""
    cbo_big_all_yn = ""
End Sub

Public Sub Form_Del()

    If Not Gf_Ms_Del(M_CN1, Mc1) Then Call MDIMain.FormMenuSetting(Me, FormType, "FS", sAuthority)
    
End Sub

