Public Sub Form_Pro()
    Dim s_olc, sMesg As String
    Dim i As Integer
    Dim hmps_wgt(14) As Integer
    
    
    hmps_wgt(0) = hmps_wgt1.VALUE
    hmps_wgt(1) = hmps_wgt2.VALUE
    hmps_wgt(2) = hmps_wgt3.VALUE
    hmps_wgt(3) = hmps_wgt4.VALUE
    hmps_wgt(4) = hmps_wgt5.VALUE
    hmps_wgt(5) = hmps_wgt6.VALUE
    hmps_wgt(6) = hmps_wgt7.VALUE
    hmps_wgt(7) = hmps_wgt8.VALUE
    hmps_wgt(8) = hmps_wgt9.VALUE
    hmps_wgt(9) = hmps_wgt10.VALUE
   hmps_wgt(10) = hmps_wgt11.VALUE
   hmps_wgt(11) = hmps_wgt12.VALUE
   hmps_wgt(12) = hmps_wgt13.VALUE
   hmps_wgt(13) = hmps_wgt14.VALUE
   hmps_wgt(14) = hmps_wgt15.VALUE
         
     For i = 0 To 14
         If chk(i).VALUE = ssCBChecked Then
            If cbo_olc(i) = "" Or Len(cbo_olc(i).Text) <> 3 Or (Mid(cbo_olc(i).Text, 1, 1) <> "F" And Mid(cbo_olc(i).Text, 1, 1) <> "L") Then
                MsgBox "炼铁铁包号 " + cbo_olc(i).Text + " 错误！", vbCritical, "错误提示"
                cbo_olc(i).SetFocus
                Exit Sub
            ElseIf sdb_olc_temp(i).Text = "" Or sdb_olc_wgt(i) = "" Or stx_train_arrv(i).RawData = "" Then
                MsgBox "请输入铁包号 " + cbo_olc(i) + " 的受铁量,铁水温度和到达时间！", vbCritical, "错误提示"
                sdb_olc_temp(i).SetFocus
                Exit Sub
            End If
         Else
            cbo_olc(i).Text = ""
         End If
         
         'add by yidujun at 2010-12-20 检查受铁量和倒空量之差是否超过30t，超过提示是否确实要保存
         If hmps_wgt(i) <> 0 Then
            If Abs(sdb_olc_wgt(i).VALUE - hmps_wgt(i)) > 30 Then
               If Not Gf_MessConfirm("炼铁铁包号 " + cbo_olc(i) + "受铁量和倒空量之差超过30t,您确定要保存数据吗？", "Q") Then Exit Sub
            End If
         End If
         
     Next i
     '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

     If Gf_Mc_Authority(sAuthority, Mc1) Then
           txt_emp_cd.Text = sUserID
           If Gf_Ms_Process(M_CN1, Mc1, sAuthority) Then
                Call MDIMain.FormMenuSetting(Me, FormType, "SE", sAuthority)
                Call MenuTool_ReSet
           End If
     End If
     v_bf_plant = cbo_bf_plant
     'Call Form_pro_Cls
     Call Form_Ref
End Sub

