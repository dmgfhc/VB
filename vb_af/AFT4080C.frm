Private Function Pf_Common_DD(Conn As ADODB.Connection, KeyCode As Integer) As Boolean

    Dim sOld_Code, sNew_Code  As String
    Dim sOld_Name, sNew_Name  As String
    
    If KeyCode = vbKeyReturn Or KeyCode = vbKeyEscape Or KeyCode = 229 Then
        DD.DataDicType = ""
        DD.DicRefType = ""
        DD.nameType = ""
        DD.sQuery = ""
        DD.sWitch = ""
        DD.sSelect = False
        DD.sWhere = ""
        DD.sKey = ""
        
        Set DD.rControl = Nothing
        Set DD.wControl = Nothing
        Set DD.sPname = Nothing
        Exit Function
    End If
    
    If DD.rControl.Count = 0 Or DD.rControl.Count > 2 Then
        Call Gp_MsgBoxDisplay("DataDic Condition Invaild.....", "I")
        DD.DataDicType = ""
        DD.DicRefType = ""
        DD.nameType = ""
        DD.sQuery = ""
        DD.sWitch = ""
        DD.sSelect = False
        DD.sWhere = ""
        DD.sKey = ""
        
        Set DD.rControl = Nothing
        Set DD.wControl = Nothing
        Set DD.sPname = Nothing
        Exit Function
    End If
    
    DD.DataDicType = "M"        'Common Code
    DD.DicRefType = "C"         'Active Form DataDic Call
    
    DD.sQuery = "SELECT STLGRD ""钢种代码"", STEEL_GRD_DETAIL ""钢种名称"" FROM qp_nisco_chmc"
    
    Call Gf_DD_Display(Conn, DD.sQuery, False)
    
    DD.sSelect = False
    
    Set DD.sPname = Nothing
    Set DD.rControl = Nothing

End Function

Private Sub SS1_KeyUp(KeyCode As Integer, Shift As Integer)

    Dim sTemp_Code As String
    Dim sQuery1 As String
    Dim sQuery2 As String

    If ss1.MaxRows < 1 Then Exit Sub
    
    If KeyCode = vbKeyReturn Or KeyCode = vbKeyEscape Or KeyCode = 229 Then
        Exit Sub
    End If

    Select Case ss1.ActiveCol
    
        Case 8
        
            If KeyCode = vbKeyF4 Then
            
                Set DD.sPname = Me.ss1
                
                DD.sWitch = "SP"
                DD.sKey = "F0050"
                DD.rControl.Add Item:=8
                DD.rControl.Add Item:=9
                DD.nameType = "1"
              
                Call Gf_Common_DD(M_CN1, KeyCode)
                
            Else
            
                ss1.Col = ss1.ActiveCol

                 If Len(Trim(ss1.Text)) = ss1.TypeMaxEditLen Then
                        sTemp_Code = ss1.Text
                        ss1.Col = 9
                        ss1.Text = Gf_ComnNameFind(M_CN1, "F0050", Trim(sTemp_Code), 1)
                      
                    Else
                        ss1.Col = 9
                        ss1.Text = ""
                 End If
                
            End If
            
      
    End Select
    
    ss1.Col = 8
    sQuery1 = "SELECT CD_SHORT_NAME FROM ZP_CD  WHERE CD_MANA_NO =  'F0050' AND CD = '" + Trim(ss1.Text) + "' "
    sQuery2 = "SELECT CD_NAME FROM ZP_CD  WHERE CD_MANA_NO =  'F0050' AND CD = '" + Trim(ss1.Text) + "' "
    ss1.Col = 9
    ss1.Text = Gf_CodeFind(M_CN1, sQuery1)
    ss1.Col = 10
    ss1.Text = Gf_CodeFind(M_CN1, sQuery2)
      
    ss1.Col = 12
    ss1.ROW = ss1.ActiveRow
    ss1.Text = sUserID
    
End Sub

