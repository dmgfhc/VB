Private Sub lbl_slab_DblClick(Index As Integer)
    
    Dim lRow As Integer
    
    If Slab_Cnt = Index Then
    
        For lRow = 1 To ss2.MaxRows
            ss2.Row = lRow
            ss2.Col = 1
            If ss2.Text = Ord_No.Item(Index) Then
                ss2.Col = 2
                If ss2.Text = Ord_Item.Item(Index) Then
                    ss2.Col = 36
                    ss2.Value = ss2.Value - (Val(AsRoll_Prod_Cnt.Item(Index)) * Select_SlabCnt)
                    ss2.Col = 37
                    ss2.Value = ss2.Value + (Val(AsRoll_Prod_Cnt.Item(Index)) * Select_SlabCnt)
                End If
            End If
        Next lRow
        
        If Slab_Cnt = 1 Then
            sdb_slab_rem_len.Value = sdb_slab_len.Value
            sdb_slab_rem_wgt.Value = sdb_slab_wgt.Value
        Else
            
            sdb_slab_rem_len.Value = sdb_slab_rem_len.Value + Chg_Slab_Len.Item(Index) + dSlab_Cut_Spare_Len
            sdb_slab_rem_wgt.Value = sdb_slab_rem_wgt.Value + Chg_Slab_Wgt.Item(Index) + dSlab_Cut_Spare_Wgt
            
        End If
        
        lbl_slab(Index).Visible = False
        Unload lbl_slab(Index)
        Slab_Cnt = Slab_Cnt - 1
        
        Ord_No.Remove (Index)
        Ord_Item.Remove (Index)
        Prod_Thk.Remove (Index)
        Prod_Wid.Remove (Index)
        Prod_Len.Remove (Index)
        Prod_Wgt.Remove (Index)
        Chg_Prod_Len.Remove (Index)
        Slab_Thk.Remove (Index)
        Slab_Wid.Remove (Index)
        Slab_Len.Remove (Index)
        Chg_Slab_Len.Remove (Index)
        Chg_Slab_Wgt.Remove (Index)
        Add_Slab_Len.Remove (Index)
        Add_Slab_Wgt.Remove (Index)
        Slab_Wgt.Remove (Index)
        AsRoll_Prod_Cnt.Remove (Index)
        Cnt.Remove (Index)
        Last_Cnt.Remove (Index)
        Slab_No.Remove (Index)
        Stlgrd.Remove (Index)
        AsRoll_Thk.Remove (Index)
        AsRoll_Wid.Remove (Index)
        Slab_Wid_Cut_Fl.Remove (Index)
        
    End If
    
End Sub

