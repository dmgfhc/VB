    If Select_SlabCnt <= 0 Then Exit Sub
    
    If Gf_Sp_Refer(M_CN1, sc2, Mc2) Then
    
        ss2.OperationMode = OperationModeNormal
        
        Call Gp_Sp_ColColor(ss2, 11, BLUE)
        Call Gp_Sp_ColColor(ss2, 12, RED)
        
        Call Gp_Sp_ColColor(ss2, 21, BLUE)
        Call Gp_Sp_ColColor(ss2, 22, RED)
        
        Call Gp_Sp_ColColor(ss2, 23, BLUE)
        Call Gp_Sp_ColColor(ss2, 24, RED)
        
        For iRow = 1 To ss2.MaxRows
            
            ss2.Row = iRow
            
            ss2.Col = 15
            If ss2.Text = "单定尺" Then
                ss2.Col = 12
                ss2.Lock = False
                Call Gp_Sp_CellColor(ss2, 12, iRow, RED, &HC0FFFF)
            Else
                ss2.Col = 12
                ss2.Lock = True
            End If
            
            ss2.Row = iRow

            ss2.Col = 25
            Asroll_Cnt = ss2.Value

            ss2.Col = 34
            If Asroll_Cnt > ss2.Value Then
                Call Gp_Sp_CellColor(ss2, 34, iRow, vbRed)
                Call Gp_Sp_CellColor(ss2, 36, iRow, vbRed)
                Call Gp_Sp_CellColor(ss2, 37, iRow, vbRed)
            End If
        
            ss2.Row = iRow
            
            For iCnt = 1 To Slab_Cnt
            
                ss2.Col = 1
