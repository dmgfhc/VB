                    ss2.Col = 2
                    If Ord_Item.Item(iCnt) = ss2.Text Then
                    
                        '替代产品数
                        ss2.Col = 36
                        ss2.Value = ss2.Value + (AsRoll_Prod_Cnt.Item(iCnt) * Select_SlabCnt)
                        '替代剩余产品数
