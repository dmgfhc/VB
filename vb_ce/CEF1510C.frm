Private Sub ss2_EditMode(ByVal Col As Long, ByVal Row As Long, ByVal Mode As Integer, ByVal ChangeMade As Boolean)

    Dim Prod_Thk As Double
    Dim Prod_Wid As Double
    Dim Prod_Len As Double
