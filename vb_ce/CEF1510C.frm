    Dim sStlgrd As String
    
    If Col <> 12 Then Exit Sub
    
    ss2.Row = Row
    
    If ChangeMade Then
    
        'EndUse
        ss2.Col = 4
        sENDUSE_CD = ss2.Text
        
        'StlGrd
        ss2.Col = 5
        sStlgrd = ss2.Text
        
        'Prod
        ss2.Col = 9
        Prod_Thk = ss2.Value
        
        ss2.Col = 10
        Prod_Wid = ss2.Value
        
        ss2.Col = 11
        Prod_Len = ss2.Value
        
        ss2.Col = 12
        Chg_Prod_Len = ss2.Value
        Ext_Prod_Len = Chg_Prod_Len - Prod_Len
        
        ss2.Col = 38
        Max_Prod_Len = ss2.Value
        
        'Slab
        ss2.Col = 19
        Slab_Thk = ss2.Value
        
        ss2.Col = 20
        Slab_Wid = ss2.Value
        
        ss2.Col = 23
        Slab_Wgt = ss2.Value
        
        ss2.Col = 25
        Asroll_Cnt = ss2.Value
        
        ss2.Col = 39
        Slab_wgt_mm = ss2.Value
        
        ss2.Col = 40
        Max_Slab_Wgt = ss2.Value
        
        'Ext_Slab_Wgt
        Ext_Slab_Wgt = (Ext_Prod_Len * Slab_wgt_mm) / 1000
        Chg_Slab_Wgt = Slab_Wgt + (Ext_Slab_Wgt * Asroll_Cnt)
        
        'Chg_Slab_Len
        ss2.Col = 22
        Chg_Slab_Len = Gf_FloatFind(M_CN1, "SELECT GF_JP_WGT('LEN','" & Select_Slab_Stlgrd & "'," & sdb_slab_thk1.Value & "," & sdb_slab_wid1.Value & ",0," & Chg_Slab_Wgt & ") FROM DUAL ")
        ss2.Value = Chg_Slab_Len
        
        'Chg_Slab_Wgt
        ss2.Col = 24
        Chg_Slab_Wgt = Gf_FloatFind(M_CN1, "SELECT GF_JP_WGT('WGT','" & Select_Slab_Stlgrd & "'," & sdb_slab_thk1.Value & "," & sdb_slab_wid1.Value & "," & Chg_Slab_Len & ",0) FROM DUAL ")
        ss2.Value = Chg_Slab_Wgt
        
        'Prod_Wgt
        Prod_Wgt = Gf_FloatFind(M_CN1, "SELECT GF_CAL_PLATE_WGT('WGT','" & sENDUSE_CD & "','" & Select_Slab_Stlgrd & "'," & Prod_Thk & "," & Prod_Wid & "," & Chg_Prod_Len & ",0) FROM DUAL ")
        
        ss2.Col = 18
        ss2.Value = Prod_Wgt
        
        'Ord_Rem_Wgt
        ss2.Col = 33
        Ord_Rem_Wgt = ss2.Value
        
        'Ord_Rem_Cnt
        Ord_Rem_Cnt = Gf_FloatFind(M_CN1, "SELECT TRUNC(" & Ord_Rem_Wgt & " / " & Prod_Wgt & ") FROM DUAL")
        
        ss2.Col = 34
        ss2.Value = Ord_Rem_Cnt
        ss2.Col = 35
        ss2.Value = Ord_Rem_Cnt
        ss2.Col = 37
        ss2.Value = Ord_Rem_Cnt
        
    End If
    
End Sub

Private Sub ss2_LeaveCell(ByVal Col As Long, ByVal Row As Long, ByVal NewCol As Long, ByVal NewRow As Long, Cancel As Boolean)

    Dim iCol As Integer
    Dim iRow As Integer
    Dim dMin As Double
    Dim dMax As Double
    Dim Cur_Len As Double
    Dim Max_Len As Double
    Dim cValue As Double
    Dim sQuery As String
   
    If Row < 0 Or Row = 0 Then Exit Sub
    
    With ss2
            
        If .CellTag = "False" Then Exit Sub
        
        .Row = Row
              
        Select Case Col
        
            Case 12      'Design Product Len
            
                .Col = Col
                cValue = .Value
                
                .Col = Col - 1
                If .Value = "" Then
                    Cur_Len = 0
                Else
                    Cur_Len = .Value
                End If
                
                .Col = Col + 1
                If .Value = "" Then
                    Max_Len = 0
                Else
                    Max_Len = .Value
                End If
                
                .Col = Col + 4
                If .Value = "" Then
                    dMin = 0
                Else
                    dMin = .Value
                End If
                
                .Col = Col + 5
                If .Value = "" Then
                    dMax = 0
                Else
                    dMax = .Value
                End If
                                
                If cValue > dMax Or cValue < dMin Or cValue < (Cur_Len - 2000) Or Max_Len < cValue Then
                
                    .Col = Col
                    .Row = Row
                    .CellTag = "False"
                 
                    Call Gp_MsgBoxDisplay("已超出最大/最小值...!!")
                  
                    .Col = Col
                    .Row = Row
                    .CellTag = ""
                    
                    .Value = 0
                    .TabStop = True
                    .SetFocus
                    .SetActiveCell Col, Row
                    .Action = SS_ACTION_ACTIVE_CELL
                    .EditMode = True
                    .TabStop = False
                    
                End If
           
        End Select
            
   End With

End Sub

