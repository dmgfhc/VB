Private Function Sp_Process(Conn As ADODB.Connection, Sc As Collection) As Boolean

On Error GoTo SpreadPro_Error

    Dim iCol, iCount, iProcessCount As Integer
    Dim iRow As Integer
    Dim ret_Result_ErrCode As Integer
    Dim ret_Result_ErrMsg As String
    Dim lRow As Integer
    Dim v_Slab_No As String
    
    Dim adoCmd As ADODB.Command

    Sp_Process = True
    
    Screen.MousePointer = vbHourglass
    
    'Db Connection Check
    If Conn.State = 0 Then
        If GF_DbConnect = False Then Sp_Process = False: Exit Function
    End If
    
    'Ado Setting
    Conn.CursorLocation = adUseServer
    Set adoCmd = New ADODB.Command
    
    Set adoCmd.ActiveConnection = Conn
    adoCmd.CommandType = adCmdStoredProc
    adoCmd.CommandText = Sc.Item("P-M")
    
    Conn.BeginTrans
    
    'Create Parameter (Input) iType + iColumn
    For iCount = 0 To 21
        adoCmd.Parameters.Append adoCmd.CreateParameter("", adVariant, adParamInput)
    Next iCount
    
    'Create Parameter (Output)
    adoCmd.Parameters.Append adoCmd.CreateParameter("Error", adVariant, adParamOutput)
    adoCmd.Parameters.Append adoCmd.CreateParameter("Messg", adVariant, adParamOutput)
    
    For lRow = 1 To ss1.MaxRows
    
        ss1.Row = lRow
        ss1.Col = 0
        
        If ss1.Text <> "" Then
        
        
            ss1.Col = 1
            v_Slab_No = ss1.Text
            
            ss1.Col = 22
    
            For iCount = 1 To Slab_Cnt
            
                adoCmd.Parameters(0).Value = "U"
                adoCmd.Parameters(1).Value = Ord_No.Item(iCount)
                adoCmd.Parameters(2).Value = Ord_Item.Item(iCount)
                adoCmd.Parameters(3).Value = Val(Prod_Thk.Item(iCount))
                adoCmd.Parameters(4).Value = Val(Prod_Wid.Item(iCount))
                adoCmd.Parameters(5).Value = Val(Prod_Len.Item(iCount))
                adoCmd.Parameters(6).Value = Val(Chg_Prod_Len.Item(iCount))
                adoCmd.Parameters(7).Value = Val(Slab_Thk.Item(iCount))
                adoCmd.Parameters(8).Value = Val(Slab_Wid.Item(iCount))
                adoCmd.Parameters(9).Value = Val(Slab_Len.Item(iCount))
                adoCmd.Parameters(10).Value = Val(Chg_Slab_Len.Item(iCount))
                adoCmd.Parameters(11).Value = Val(Slab_Wgt.Item(iCount))
                adoCmd.Parameters(12).Value = Val(Chg_Slab_Wgt.Item(iCount))
                adoCmd.Parameters(13).Value = Val(AsRoll_Prod_Cnt.Item(iCount))
                adoCmd.Parameters(14).Value = Val(Add_Slab_Len.Item(iCount))
                adoCmd.Parameters(15).Value = Val(Add_Slab_Wgt.Item(iCount))
                adoCmd.Parameters(16).Value = v_Slab_No   'Slab_No
                adoCmd.Parameters(17).Value = sUserID
                adoCmd.Parameters(18).Value = Val(Cnt.Item(iCount))
                adoCmd.Parameters(19).Value = IIf(iCount < Slab_Cnt, 0, Slab_Cnt)
                adoCmd.Parameters(20).Value = Val(Slab_Wid_Cut_Fl.Item(iCount))
                adoCmd.Parameters(21).Value = Val(ss1.Text)
                
                adoCmd.Execute
                    
                'Error Check
                If adoCmd("Error") <> "0" Then
                
                    ret_Result_ErrCode = adoCmd("Error")
                    ret_Result_ErrMsg = adoCmd("Messg")
            
                    sErrMessg = "Error Code : " & ret_Result_ErrCode & vbCrLf & "Error Mesg : " & ret_Result_ErrMsg
                    
                    For iRow = 1 To ss2.MaxRows
                    
                        ss2.Row = iRow
                        ss2.Col = 1
                        
                        If ss2.Text = Ord_No.Item(iCount) Then
                            
                            ss2.Col = 2
                            
                            If ss2.Text = Ord_Item.Item(iCount) Then
                                Call Gp_Sp_RowColor(ss2, lRow, , vbYellow)
                            End If
                        
                        End If
                        
                    Next iRow
                    
                    Call Gp_MsgBoxDisplay(sErrMessg)
                    
                    Screen.MousePointer = vbDefault
                    Set adoCmd = Nothing
                    
                    Conn.RollbackTrans
                    Sp_Process = False
                    Exit Function
            
                End If
                
                iProcessCount = iProcessCount + 1
                
            Next iCount
            
        End If
        
    Next lRow
    
    Conn.CommitTrans
    
    Sc.Item("Spread").ReDraw = True
    
    If iProcessCount > 0 Then
        
        MDIMain.StatusBar1.Panels(1) = "提示信息 ：成功处理了" & iProcessCount & "条记录"
        
    End If
            
    Set adoCmd = Nothing
    Screen.MousePointer = vbDefault
    Exit Function

SpreadPro_Error:
    
    Set adoCmd = Nothing
    Conn.RollbackTrans
    Sp_Process = False
    Call Gp_MsgBoxDisplay("Gf_Sp_Process Error : " & Error)
    Screen.MousePointer = vbDefault

End Function

