Public Sub Spread_Del()
    
    Dim lRow As Integer
    
    Call Gp_Sp_Del(Proc_Sc("SC"))
    
    For lRow = 1 To ss1.MaxRows
    
        ss1.Row = lRow
        ss1.Col = 0
        
        If ss1.Text = "Delete" Then
            ss1.Col = 26
            
            If ss1.Value > 0 Then
                ss1.Col = 0
                ss1.Text = ""
            End If
            
        End If
        
    Next lRow

