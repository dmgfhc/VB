Private Sub lbl_slab_DblClick(Index As Integer)
    
    Dim Sw As String
    Dim lRow As Integer
    Dim iIdx As Integer
    Dim iOrd_Cnt As Integer
    Dim iProd_Knd_Cnt As Integer
    Dim Prod_Cnt As Integer
    Dim sOrd_No As String
    Dim dProd_Wgt As Double
    Dim dTotprod_Wgt As Double
    Dim dDesign_Ratio As Double
    
    If Slab_Cnt = Index Then
    
        For lRow = 1 To ss2.MaxRows
            ss2.Row = lRow
            ss2.Col = 1
            If ss2.Text = Ord_No.Item(Index) Then
                ss2.Col = 2
                If ss2.Text = Ord_Item.Item(Index) Then
                    ss2.Col = 31
                    ss2.Value = ss2.Value - (Val(AsRoll_Prod_Cnt.Item(Index)) * Select_SlabCnt)
                    ss2.Col = 32
                    ss2.Value = ss2.Value + (Val(AsRoll_Prod_Cnt.Item(Index)) * Select_SlabCnt)
                End If
            End If
        Next lRow
        
        lbl_slab(Index).Visible = False
        Unload lbl_slab(Index)
        
        If Slab_Cnt = 1 Then
            sdb_ord_wgt.Value = 0
            sdb_div_wgt.Value = sdb_mat_wgt.Value
        Else
            sdb_ord_wgt.Value = sdb_ord_wgt.Value - Val(Slab_Wgt.Item(Index)) - dSlab_Cut_Spare_Wgt
            sdb_div_wgt.Value = sdb_div_wgt.Value + Val(Slab_Wgt.Item(Index)) + dSlab_Cut_Spare_Wgt
        End If
        
        Slab_Cnt = Slab_Cnt - 1
        
        Ord_No.Remove (Index)
        Ord_Item.Remove (Index)
        Prod_Thk.Remove (Index)
        Prod_Wid.Remove (Index)
        Prod_Len.Remove (Index)
        Prod_Wgt.Remove (Index)
        Slab_Thk.Remove (Index)
        Slab_Wid.Remove (Index)
        Slab_Len.Remove (Index)
        Slab_Wgt.Remove (Index)
        Ord_Slab_Thk.Remove (Index)
        Ord_Slab_Wid.Remove (Index)
        Ord_Slab_Len.Remove (Index)
        AsRoll_Prod_Cnt.Remove (Index)
        AsRoll_Thk.Remove (Index)
        AsRoll_Wid.Remove (Index)
        Design_Ratio.Remove (Index)
        Slab_No.Remove (Index)
        Stlgrd.Remove (Index)
        
        Sw = "Y"
        Prod_Cnt = 0
        For iIdx = 1 To Slab_Cnt
        
            If sOrd_No <> Ord_No.Item(iIdx) & Ord_Item.Item(iIdx) Then
            
                If Sw = "Y" Then
                    Sw = "N"
                    iOrd_Cnt = 1
                Else
                    iOrd_Cnt = iOrd_Cnt + 1
                End If
                
                sOrd_No = Ord_No.Item(iIdx) & Ord_Item.Item(iIdx)
                
            End If
            
            If Prod_Cnt = 0 Then
                Prod_Cnt = AsRoll_Prod_Cnt.Item(iIdx)
                iProd_Knd_Cnt = 1
            Else
                If Prod_Cnt <> AsRoll_Prod_Cnt.Item(iIdx) Then
                    iProd_Knd_Cnt = iProd_Knd_Cnt + 1
                    Prod_Cnt = AsRoll_Prod_Cnt.Item(iIdx)
                End If
            End If
            
            dDesign_Ratio = Design_Ratio.Item(iIdx)
            dTotprod_Wgt = dTotprod_Wgt + (Prod_Wgt.Item(iIdx) * AsRoll_Prod_Cnt.Item(iIdx))
            
        Next iIdx
        
        If iOrd_Cnt = 1 And iProd_Knd_Cnt = 1 Then
            sdb_ord_rate.Value = dDesign_Ratio
        Else
            sdb_ord_rate.Value = 0
        End If
        
        sdb_mat_rate.Value = Round((dTotprod_Wgt / sdb_mat_wgt.Value) * 100, 1)
        
        '差成材率 = 设计成材率 - 标准成材率
        sdb_rate.Value = sdb_mat_rate.Value - sdb_ord_rate.Value
        
    End If
    
End Sub

