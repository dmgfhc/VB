            ss1.Col = 9
            If Select_Slab_Len < ss1.Value - 10 Or Select_Slab_Len > ss1.Value + 10 Then Exit Sub
            
            ss1.Col = 21
            If Select_Slab_Wgt < ss1.Value - 0.04 Or Select_Slab_Wgt > ss1.Value + 0.04 Then Exit Sub
            
