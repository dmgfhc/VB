    ss1.Row = iRow
    
    ss1.Col = 10
    lMinValue = Val(ss1.Value)
    ss1.Col = 11
    lMaxValue = Val(ss1.Value)
    ss1.Col = 12
    lTgtValue = Val(ss1.Value)
    
    If lMinValue <> 0 Or lMaxValue <> 0 Or lTgtValue <> 0 Then
