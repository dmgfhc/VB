            txt_MLT_PROC_CD = "******"
        End If
        If Mid(.Text, 1, 2) = "**" Or Mid(.Text, 1, 2) = "BD" Or Mid(.Text, 1, 2) = "BE" Or Mid(.Text, 1, 2) = "BG" Then
            Select Case Mid(.Text, 1, 2)
                Case "**"
                    cob_MLT_PROC_CD_1.Text = "**"
                Case "BG"
                    cob_MLT_PROC_CD_1.Text = "CAS"
                Case "BD"
                    cob_MLT_PROC_CD_1.Text = "LF"
                Case "BE"
                    cob_MLT_PROC_CD_1.Text = "VD"
