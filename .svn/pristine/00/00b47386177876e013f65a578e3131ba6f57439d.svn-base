'必须输入项目检查
Private Function Sp_NecessaryCheck(iRow As Long) As Boolean
    
'-------Modify 07.04.05 Kim sung ho

    Dim sProc_Line As String
    Dim sProcLab As String
    Dim sProcLab2 As String
    Dim iCheck  As Integer
    
    Sp_NecessaryCheck = False
    
'钢种
    If GF_Sp_Necessary_Value_Check(Sc1, iRow, 3, ULabel3(4).Caption, txt_STLGRD) = False Then Exit Function

    '精炼工艺路径
'    If GF_Sp_Necessary_Value_Check(sc1, iRow, 9, ULabel3.Item(5).Caption, cob_MLT_PROC_CD_1) = False _
'    And GF_Sp_Necessary_Value_Check(sc1, iRow, 22, ULabel3.Item(15).Caption, cob_MLT_PROC_CD_2) = False Then
'        Exit Function
'    Else
        ss1.Row = iRow
        
        ss1.Col = 7
        sProc_Line = ss1.Text
        
        ss1.Col = 9
        sProcLab = ss1.Text
   
        ss1.Col = 22
        sProcLab2 = ss1.Text
'    End If
    
    '铸坯去向
    If GF_Sp_Necessary_Value_Check(Sc1, iRow, 6, ULabel3.Item(6).Caption, txt_HCR_KND) = False _
    And GF_Sp_Necessary_Value_Check(Sc1, iRow, 6, ULabel3.Item(17).Caption, txt_HCR_KND2) = False Then Exit Function
    
    '转炉终点温度(出钢温度), 中间包温度 - 最小、目标、最大
    If sProc_Line = "1" Or sProc_Line = "A" Or sProc_Line = "B" Or _
       InStr(1, Trim(sProcLab), "BD") > 0 Or _
       InStr(1, Trim(sProcLab), "BE") > 0 Then
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 10, ULabel3(0).Caption + "最小值", txt_MLT_TMP_MIN) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 11, ULabel3(0).Caption + "最大值", txt_MLT_TMP_MAX) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 12, ULabel3(0).Caption + "目标值", txt_MLT_TMP_TGT) = False Then Exit Function
       
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 19, ULabel3(3).Caption + "最小值", txt_TD_TMP_MIN) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 20, ULabel3(3).Caption + "最大值", txt_TD_TMP_MAX) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 21, ULabel3(3).Caption + "目标值", txt_TD_TMP_TGT) = False Then Exit Function
    End If
    
    If sProc_Line = "2" Or sProc_Line = "A" Or sProc_Line = "B" Or _
       InStr(1, Trim(sProcLab2), "BD") > 0 Or _
       InStr(1, Trim(sProcLab2), "BH") > 0 Then
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 23, ULabel3(12).Caption + "最小值", txt_MLT_TMP_MIN2) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 24, ULabel3(12).Caption + "最大值", txt_MLT_TMP_MAX2) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 25, ULabel3(12).Caption + "目标值", txt_MLT_TMP_TGT2) = False Then Exit Function
       
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 32, ULabel3(14).Caption + "最小值", txt_TD_TMP_MIN2) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 33, ULabel3(14).Caption + "最大值", txt_TD_TMP_MAX2) = False Then Exit Function
       If GF_Sp_Necessary_Value_Check(Sc1, iRow, 34, ULabel3(14).Caption + "目标值", txt_TD_TMP_TGT2) = False Then Exit Function
    End If

    '精炼工艺路径中是否包含LF,如果包含则检查
    If InStr(1, Trim(sProcLab), "BD") > 0 Then
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 13, ULabel3(1).Caption + "最小值", txt_LF_TEMP_MIN) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 14, ULabel3(1).Caption + "最大值", txt_LF_TEMP_MAX) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 15, ULabel3(1).Caption + "目标值", txt_LF_TEMP_TGT) = False Then Exit Function
    End If
    If InStr(1, Trim(sProcLab2), "BD") > 0 Then
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 26, ULabel3(13).Caption + "最小值", txt_LF_TEMP_MIN2) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 27, ULabel3(13).Caption + "最大值", txt_LF_TEMP_MAX2) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 28, ULabel3(13).Caption + "目标值", txt_LF_TEMP_TGT2) = False Then Exit Function
    End If
    
    '精炼工艺路径中是否包含VD,如果包含则检查
    If InStr(1, Trim(sProcLab), "BE") > 0 Then
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 16, ULabel3(7).Caption + "最小值", txt_VD_TMP_MIN) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 17, ULabel3(7).Caption + "最大值", txt_VD_TMP_MAX) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 18, ULabel3(7).Caption + "目标值", txt_VD_TMP_TGT) = False Then Exit Function
    End If
    
    '精炼工艺路径中是否包含RH,如果包含则检查
    If InStr(1, Trim(sProcLab2), "BH") > 0 Then
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 29, ULabel3(16).Caption + "最小值", txt_RH_TMP_MIN) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 30, ULabel3(16).Caption + "最大值", txt_RH_TMP_MAX) = False Then Exit Function
        If GF_Sp_Necessary_Value_Check(Sc1, iRow, 31, ULabel3(16).Caption + "目标值", txt_RH_TMP_TGT) = False Then Exit Function
    End If
    
    Sp_NecessaryCheck = True

End Function

Private Sub Data_Copy(ByVal sVa As vaSpread, ByVal iRow As Long, ByVal o_From As Object, ByVal o_To As Object)
    Dim iCheck As Integer
    
    With sVa
        .Col = 0
        .Row = iRow
        If .Text = "Update" Or .Text = "Insert" Then
            iCheck = 1
        Else
            iCheck = 0
        End If
    End With
    
    If iCheck = 0 Then Exit Sub
    
    If TypeName(o_From) = "sidbEdit" And TypeName(o_To) = "sidbEdit" Then
        o_To.Text = o_From.Text
    Else
        Exit Sub
    End If
End Sub

Private Sub Change_PROC_CD(ByVal sText As String, ByVal sCD As String, ByVal iLOC As Integer, ByVal oTEXT_OBJ As Object)
               
        If Len(sText) >= 6 Then
            Mid(sText, iLOC, 2) = sCD
        Else
            If iLOC = 1 Then
                sText = sCD
            Else
                sText = Mid(sText, 1, iLOC + 1) + sCD
            End If
        End If
        
        If TypeName(oTEXT_OBJ) = "TextBox" Then
            oTEXT_OBJ.Text = sText
        End If
End Sub
