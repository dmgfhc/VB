Public Sub Form_Pro()
    Dim iMaxrow As Long
    Dim iRow As Long
    Dim icount As Long
    Dim strProc As String
    
    iRow = ss1.Row
    iMaxrow = ss1.MaxRows
    
    ss1.ReDraw = False
    
    For icount = 1 To ss1.MaxRows
         ss1.Col = 0
         ss1.Row = icount
        Select Case ss1.Text
            
            Case "Input", "Update"
             
             If Sp_NecessaryCheck(icount) = False Then          '必须输入项检查
                Call Spread_to_Master(ss1, icount)
                Exit Sub
             End If                                             '最大值,最小值,目标值检查
             
             If Sp_subMinMaxValueCheck(icount) = False Then
                Call Spread_to_Master(ss1, icount)
                Exit Sub
             End If
             
'            ss1.Col = 20
'            strProc = ss1.Text
        End Select
        
    Next icount
    
    ss1.ReDraw = True
    
    If Gf_Mc_Authority(sAuthority, Mc1) Then
                
        If Gf_Sp_Process(M_CN1, Proc_Sc("SC"), Mc1) Then
            Call MDIMain.FormMenuSetting(Me, FormType, "SE", sAuthority)
            Call Gp_Goto_Row(ss1, iMaxrow, iRow)
            Call Spread_to_Master(ss1, iRow)
        End If

    End If
    
End Sub

