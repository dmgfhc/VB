'---------------------------------------------------------------------------------------------------------------------------------------------
Private Sub Form_KeyUp(KeyCode As Integer, Shift As Integer)
On Error GoTo Err_Track:
    Dim oCodeName As Object
    Dim sCode As String
    
    If Not (KeyCode = vbKeyF4) Then Exit Sub
    
    Select Case Me.ActiveControl.Name
                        
        Case "txt_STLGRD"   '钢种
            sCode = "STLGRD"
            Set oCodeName = txt_STLGRD_DETAIL
                
        Case "txt_HCR_KND"    '1#铸坯去向
            sCode = "C0005"
            Set oCodeName = txt_HCR_NAME
            
        Case "txt_HCR_KND2"    '2#铸坯去向
            sCode = "C0005"
            Set oCodeName = txt_HCR_NAME2
    
            
         Case "txt_MLT_STD_NO_P"    '规范编号
            sCode = "MLT_STD_NO"
            Set oCodeName = txt_APP_DATE_P
            
        Case "txt_MLT_STD_NO" ', "txt_MLT_STD_NO_P"    '规范编号
            sCode = "MLT_STD_NO"
            Set oCodeName = txt_APP_DATE
           ' txt_APP_DATE.Text = Format(txt_APP_DATE, "####-##-##")
    End Select
    
    If sCode = "" Then Exit Sub
    
    Call Gp_MS_CodeNameFind(KeyCode, sCode, Me.ActiveControl, oCodeName)
    
    Set oCodeName = Nothing
Err_Track:
End Sub


