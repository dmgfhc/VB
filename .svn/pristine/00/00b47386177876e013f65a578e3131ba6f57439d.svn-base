    With cob_MLT_PROC_CD_2
        Select Case sCD1
            Case "**"
                .Clear
                Call .AddItem("**", 0)
            Case "CAS"
                .Clear
                Call .AddItem("**", 0)
                Call .AddItem("LF", 1)
                Call .AddItem("VD", 2)
            Case "LF"
                .Clear
                Call .AddItem("**", 0)
                Call .AddItem("VD", 1)
            Case "VD"
                .Clear
                Call .AddItem("**", 0)
                Call .AddItem("LF", 1)
