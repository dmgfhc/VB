Private Sub Data_Sum_Edit()
    Dim cSum()      As Double
    Dim cSumTotal() As Double
    Dim sSpecTemp   As String
    Dim sSpecTemp2   As String
    Dim dThkTemp    As Double
    Dim sSpec       As String
    Dim sSpec2      As String
    Dim dThk        As Double
    Dim iIdr        As Integer
    Dim iIdc        As Integer
    Dim iRow        As Integer
    
    ReDim cSum(5 To 16)
    ReDim cSum2(5 To 16)
    ReDim cSumTotal(4 To 16)
    
    iRow = 0
    
    With ss5
        
        For iIdr = 1 To .MaxRows
            
            iRow = iRow + 1
            
            .Row = iRow
            
            .Col = 1
            sSpec = .Text
            
           .Col = 2
            sSpec2 = .Text
            
            .Col = 3
            dThk = Val(.Text & "")
            
            If sSpecTemp <> sSpec And iRow <> 1 Then
                
                If sSpecTemp2 <> sSpec2 Then
            
                .MaxRows = .MaxRows + 1
                .InsertRows iRow, 1
                .Row = iRow
                .Col = 1:    .Text = sSpecTemp
                .Col = 2:    .Text = sSpecTemp2
                .Col = 3:    .Text = "小计"
                
                For iIdc = 5 To 16
                    
                    .Col = iIdc
                    .Text = cSum2(iIdc)
                    cSum2(iIdc) = 0
                
                Next iIdc
                
                iIdr = iIdr - 1
                
                iRow = iRow + 1
            
            End If
                
                .MaxRows = .MaxRows + 1
                .InsertRows iRow, 1
                .Row = iRow
                .Col = 1:    .Text = sSpecTemp
                .Col = 2:    .Text = "合计"
                
                For iIdc = 5 To 16
                    .Col = iIdc
                    .Text = cSum(iIdc)
                    cSum(iIdc) = 0
                Next iIdc
                
                iIdr = iIdr - 1
            
            Else
            
            
            If sSpecTemp2 <> sSpec2 And iRow <> 1 Then
            
                .MaxRows = .MaxRows + 1
                .InsertRows iRow, 1
                .Row = iRow
                .Col = 1:    .Text = sSpecTemp
                .Col = 2:    .Text = sSpecTemp2
                .Col = 3:    .Text = "小计"
                
                For iIdc = 5 To 16
                    
                    .Col = iIdc
                    .Text = cSum2(iIdc)
                    cSum2(iIdc) = 0
                
                Next iIdc
                
                iIdr = iIdr - 1
                
                iRow = iRow + 1
            
            End If
    
            For iIdc = 5 To 16
                    
                    .Row = iRow
                    .Col = iIdc
                    
                    cSum(iIdc) = cSum(iIdc) + Val(.Text & "")
                    
                    cSum2(iIdc) = cSum2(iIdc) + Val(.Text & "")
                    
                    cSumTotal(iIdc) = cSumTotal(iIdc) + Val(.Text & "")
                
            Next iIdc
            
            End If
            
                 
            sSpecTemp = sSpec
            sSpecTemp2 = sSpec2
            dThkTemp = dThk
        Next iIdr
        
        .MaxRows = .MaxRows + 1
        For iIdc = 5 To 16
'            .Row = .MaxRows - 1
'            .Col = 1:    .Text = sSpecTemp
'            .Col = 2:    .Text = "小计"
'            .Col = iIdc: .Text = cSum(iIdc)
            
            .Row = .MaxRows
            .Col = 1:    .Text = "合计"
            .Col = iIdc: .Text = cSumTotal(iIdc)
        Next iIdc
        
        ReDim cSum(1 To 6)
        
        For iIdr = 1 To .MaxRows
            .Row = iIdr
            .Col = 5:    cSum(1) = Val(.Text & "")
            .Col = 6:    cSum(2) = Val(.Text & "")
            .Col = 9:    cSum(3) = Val(.Text & "")
            .Col = 10:    cSum(4) = Val(.Text & "")
            .Col = 13:   cSum(5) = Val(.Text & "")
            .Col = 14:   cSum(6) = Val(.Text & "")
            If cSum(5) > 0 And cSum(1) > 0 Then .Col = 17: .Text = cSum(1) / cSum(5) * 100
            If cSum(6) > 0 And cSum(2) > 0 Then .Col = 18: .Text = cSum(2) / cSum(6) * 100
            'If cSum(6) > 0 Then .Col = 17:   .Text = cSum(3) / cSum(6) * 100
        Next iIdr
        
    End With

End Sub


