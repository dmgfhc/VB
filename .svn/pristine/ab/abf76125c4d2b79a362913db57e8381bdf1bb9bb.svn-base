Private Sub Form_Define()
        
    'Form Type : Start , Master, Sheet, Msheet, PopSheet, Refer
    FormType = "Refer"

       Call Gp_Ms_Collection(sdb_slab_cnt, " ", " ", " ", " ", "r", " ", " ", pControl, nControl, mControl, iControl, rControl, aControl, lControl)

    'MASTER Collection
    Mc1.Add Item:=pControl, Key:="pControl"
    Mc1.Add Item:=nControl, Key:="nControl"
    Mc1.Add Item:=mControl, Key:="mControl"
    Mc1.Add Item:=iControl, Key:="iControl"
    Mc1.Add Item:=rControl, Key:="rControl"
    Mc1.Add Item:=cControl, Key:="cControl"
    Mc1.Add Item:=aControl, Key:="aControl"
    Mc1.Add Item:=lControl, Key:="lControl"

    'Spread_Collection
    sc1.Add Item:=ss1, Key:="Spread"
    Proc_Sc.Add Item:=sc1, Key:="Sc"
    

    opt_prev = 0


    opt_area(0).Value = True
    
    Me.KeyPreview = True
    Me.BackColor = &HE0E0E0
    
End Sub

Private Sub Form_Activate()
    
    If Active_CForm <> "" Then
        

        opt_prev = 0

        opt_area(0).Value = True
        
        Call Form_Ref
        'Active_CForm = ""
        
    End If
    
    Call MDIMain.FormMenuSetting(Me, FormType, Toolbar_St, sAuthority)

End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)

    If KeyAscii = KEY_RETURN Then
        KeyAscii = 0
        SendKeys "{TAB}"
    End If

End Sub

Private Sub Form_Load()

    Screen.MousePointer = vbHourglass
    
    sAuthority = Gf_Pgm_Authority(Me.Name)
    
    Call Form_Define
    
    Call MDIMain.FormMenuSetting(Me, FormType, "FS", sAuthority)
    
    Call Gp_Ms_Cls(Mc1("rControl"))
    Call Gp_Ms_NeceColor(Mc1("nControl"))
    
    Call Sp_Setting(Proc_Sc("Sc")("Spread"))
    
    Screen.MousePointer = vbDefault
    
End Sub

Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)

    Set rControl = Nothing
    
    Set Mc1 = Nothing
    Set sc1 = Nothing
    Set Proc_Sc = Nothing
    
    Call MDIMain.FormMenuSetting(Me, "Start", Toolbar_St, "")

End Sub

Public Sub Form_Cls()

    If Gf_Sp_Cls(Proc_Sc("Sc")) Then

        opt_area(0).Value = True
        
        Call Gp_Ms_Cls(Mc1("rControl"))
        Call MDIMain.FormMenuSetting(Me, FormType, "CLS", sAuthority)
    End If
    
End Sub


Public Sub Form_Ref()

    Dim sQuery As String
    Dim SMESG As String
    Dim sArea As String
    Dim sAddr As String
    Dim iRow, iCol As Integer
    
    Dim temp_q As String
    
    
    SMESG = Gf_Ms_NeceCheck(nControl)
    
    If SMESG = "OK" Then
    
        SMESG = Gf_Ms_NeceCheck2(mControl)
        If SMESG = "OK" Then
        
            Select Case opt_prev
                    Case 0
                        sArea = "4"
                    Case 1
                        sArea = "5"
                    Case 2
                        sArea = "6"
            End Select
            
             'MaxRows Setting
            sQuery = "         SELECT COUNT(distinct(substr(yard_row,2,1)))  FROM FP_STDYARD "
            sQuery = sQuery + " WHERE PROD_TYPE = 'S' AND YARD_TYPE = '1' AND ZONE_TYPE = '" + sArea + "' AND YARD_KND = 'XK' "
            ss1.MaxRows = Val(Gf_CodeFind(M_CN1, sQuery)) * 2

            'MaxCols Setting
            sQuery = "         SELECT COUNT(YARD_COLUMN)/4  FROM FP_STDYARD "
            sQuery = sQuery + " WHERE PROD_TYPE = 'S' AND YARD_TYPE = '1'  AND ZONE_TYPE = '" + sArea + "'  AND YARD_KND = 'XK' "
            ss1.MaxCols = Val(Gf_CodeFind(M_CN1, sQuery))

            'Address Setting
            sQuery = "         SELECT LOCATION FROM FP_STDYARD "
            sQuery = sQuery + " WHERE PROD_TYPE = 'S' AND YARD_TYPE = '1'  AND ZONE_TYPE = '" + sArea + "'  AND YARD_KND = 'XK' "
            sQuery = sQuery + " ORDER BY substr(LOCATION,5,1),location "

            If Addr_Display(M_CN1, Proc_Sc("Sc"), sQuery) = False Then
                sdb_slab_cnt.Value = 0
                Exit Sub
            End If

            sQuery = "         SELECT C.ADDR, '('||C.CNT||')', D.LEN, D.WID, D.ORD_NO  "
            sQuery = sQuery + "  FROM (SELECT YARD_ADDR ADDR, COUNT(*) CNT  "
            sQuery = sQuery + "          FROM FP_SLABYARD  "
            sQuery = sQuery + "         WHERE SLAB_NO IS NOT NULL  "
            sQuery = sQuery + "           AND SUBSTR(YARD_ADDR,2,1) = '1'"
            sQuery = sQuery + "           AND SUBSTR(YARD_ADDR,3,1) = '" + sArea + "' AND YARD_KND = 'XK'  "
            sQuery = sQuery + "         GROUP BY YARD_ADDR) C,  "
            sQuery = sQuery + "        (SELECT A.LEN LEN, A.WID WID, B.YARD_ADDR ADDR, A.ORD_NO ORD_NO "
            sQuery = sQuery + "           FROM FP_SLAB A, FP_SLABYARD B  "
            sQuery = sQuery + "          WHERE A.SLAB_NO = B.SLAB_NO"
            sQuery = sQuery + "            AND YARD_ADDR || BED_SEQ IN   "
            sQuery = sQuery + "                                        (SELECT YARD_ADDR||MAX(BED_SEQ)  "
            sQuery = sQuery + "                                           FROM FP_SLABYARD  "
            sQuery = sQuery + "                                          WHERE SLAB_NO IS NOT NULL  "
           sQuery = sQuery + "                                             AND SUBSTR(YARD_ADDR,3,1) = '" + sArea + "' AND YARD_KND = 'XK' "
            sQuery = sQuery + "                                          GROUP BY YARD_ADDR )) D "
            sQuery = sQuery + " WHERE C.ADDR = D.ADDR  "
            sQuery = sQuery + " ORDER BY C.ADDR  "

            If Only_Display(M_CN1, Proc_Sc("Sc"), sQuery) Then
               Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
               sdb_slab_cnt.Value = Gf_FloatFind(M_CN1, _
                                    "SELECT COUNT(*) FROM FP_SLABYARD WHERE SUBSTR(YARD_ADDR,3,1) = '" + sArea + "'AND NVL(SLAB_NO,' ') <> ' ' AND YARD_KND = 'XK'")
            Else
               sdb_slab_cnt.Value = 0
            End If

            If Active_CForm <> "" Then
                With ss1
                    .OperationMode = OperationModeNormal

                     For iRow = 2 To .MaxRows Step 2
                         .Row = iRow
                          For iCol = 1 To .MaxCols
                              .Col = iCol
                               If Trim(.TypeButtonText) = "" Then
                                 .TypeButtonColor = &HC0C0FF
                               End If
                          Next iCol
                     Next iRow
                End With
            End If

        Else
            SMESG = SMESG + "长度不正确"
            Call Gp_MsgBoxDisplay(SMESG)
        End If

    Else
        SMESG = SMESG + "必须输入"
        Call Gp_MsgBoxDisplay(SMESG)
    End If
    

End Sub

