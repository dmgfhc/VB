    Call Gp_Spl_SizeGet(SSSplitter3, "Z-System.INI", Me.Name, "H")
    
    Call Gp_Sp_ColGet(Sc1.Item("Spread"), "Z-System.INI", Me.Name)
    Call Gp_Sp_ColGet(Sc2.Item("Spread"), "Z-System.INI", Me.Name)
    
    Screen.MousePointer = vbDefault

End Sub

