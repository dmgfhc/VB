Private Sub cmd_up_Click()

    Dim V_HEAT_NO As String
  
    If Trim(cbo_heat_no.Text) = "" Or (Len(Trim(cbo_heat_no.Text)) <> 8 And Len(Trim(cbo_heat_no.Text)) <> 9) Then
        Exit Sub
    End If

    If Len(Trim(cbo_heat_no.Text)) = 8 Then
  
        If Mid(cbo_heat_no, 4, 5) = "99999" Then Exit Sub
        
        V_HEAT_NO = Mid(cbo_heat_no.Text, 1, 3) & Format(Mid(cbo_heat_no.Text, 4, 5) + 1, "00000")
  
    ElseIf Len(Trim(cbo_heat_no.Text)) = 9 Then
    
        If Mid(cbo_heat_no, 6, 4) = "9999" Then Exit Sub
        
        V_HEAT_NO = Mid(cbo_heat_no.Text, 1, 5) & Format(Mid(cbo_heat_no.Text, 6, 4) + 1, "0000")
        
    End If
  
    Call Form_Cls
    cbo_heat_no.Text = V_HEAT_NO
    Call Form_Ref

End Sub

Private Sub cmd_down_Click()

    Dim V_HEAT_NO As String
  
    If Trim(cbo_heat_no.Text) = "" Or (Len(Trim(cbo_heat_no.Text)) <> 8 And Len(Trim(cbo_heat_no.Text)) <> 9) Then
        Exit Sub
    End If

    If Len(Trim(cbo_heat_no.Text)) = 8 Then
  
        If Mid(cbo_heat_no, 4, 5) = "00001" Then Exit Sub
        
        V_HEAT_NO = Mid(cbo_heat_no.Text, 1, 3) & Format(Mid(cbo_heat_no.Text, 4, 5) - 1, "00000")
  
    ElseIf Len(Trim(cbo_heat_no.Text)) = 9 Then
    
        If Mid(cbo_heat_no, 6, 4) = "0001" Then Exit Sub
        
        V_HEAT_NO = Mid(cbo_heat_no.Text, 1, 5) & Format(Mid(cbo_heat_no.Text, 6, 4) - 1, "0000")
        
    End If
  
    Call Form_Cls
    cbo_heat_no.Text = V_HEAT_NO
    Call Form_Ref
    
End Sub

