Private Sub Form_Activate()

    Call MDIMain.FormMenuSetting(Me, FormType, Toolbar_St, sAuthority)

End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)

    If KeyAscii = KEY_RETURN Then
        KeyAscii = 0
        SendKeys "{TAB}"
    End If

End Sub

Private Sub Form_Load()

    Screen.MousePointer = vbHourglass

    sAuthority = Gf_Pgm_Authority(Me.Name)

    Call Form_Define

    Call MDIMain.FormMenuSetting(Me, FormType, "FS", sAuthority)

    Call Gp_Ms_Cls(Mc1("rControl"))

    Call Gp_Ms_ControlLock(Mc1("lControl"), True)

    Call Gp_Ms_NeceColor(Mc1("nControl"))
    
    Call Gp_Sp_Setting(Proc_Sc("Sc")("Spread"))
    
    Call Gf_Sp_Cls(Proc_Sc("Sc"))
    
    Call Gp_Sp_ColGet(Proc_Sc("Sc")("Spread"), "G-System.INI", Me.Name)
    
    SDT_PROD_DATE_FR.RawData = Gf_DTSet(M_CN1, "D")
    SDT_PROD_DATE_TO.RawData = Gf_DTSet(M_CN1, "D")
    
    Screen.MousePointer = vbDefault

End Sub

Private Sub Form_Unload(Cancel As Integer)

    Call Gp_Sp_ColSet(Proc_Sc("Sc")("Spread"), "G-System.INI", Me.Name)
    
    Set pControl = Nothing
    Set nControl = Nothing
    Set iControl = Nothing
    Set rControl = Nothing
    Set cControl = Nothing
    Set aControl = Nothing
    Set lControl = Nothing
    Set mControl = Nothing

    Set Mc1 = Nothing
    Set sc1 = Nothing
    Set Proc_Sc = Nothing

    Call MDIMain.FormMenuSetting(Me, "Start", Toolbar_St, "")

End Sub
Public Sub Form_Exc()

    Call Gp_Sp_Excel(Me, Proc_Sc("Sc")("Spread"), lBlkcol1, lBlkcol2, lBlkrow1, lBlkrow2)

End Sub
Public Sub Form_Exit()

    Unload Me

End Sub

Public Sub Form_Cls()
    
    If Gf_Sp_Cls(sc1) Then
       Call Gp_Ms_Cls(Mc1("rControl"))
       Call MDIMain.FormMenuSetting(Me, FormType, "CLS", sAuthority)
       Call Gp_Ms_ControlLock(Mc1("lControl"), False)
    End If

End Sub

Public Sub Form_Ref()
         If Trim(SDT_PROD_DATE_FR.RawData) = "" Or Trim(SDT_PROD_DATE_TO.RawData) = "" Then
            MsgBox "查询日期未输入!", vbCritical, "系统提示信息"
            Exit Sub
         End If
        
         If Trim(SDT_PROD_DATE_FR.RawData) <> Trim(SDT_PROD_DATE_TO.RawData) Then
            MsgBox "只能查询一天内信息!", vbCritical, "系统提示信息"
            Exit Sub
         End If
    
         If Gf_Sp_ProceExist(Proc_Sc("Sc").Item("Spread")) Then Exit Sub
    
         If Gf_Sp_Refer(M_CN1, sc1, Mc1, Mc1("nControl")) Then
             Call Sum_Wgt
             ss1.OperationMode = OperationModeNormal
             Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
         End If
               
End Sub

Private Sub SDT_PAINT_DATE_GotFocus()
     If SDT_PROD_DATE_FR.RawData = "" Then
        SDT_PROD_DATE_FR.RawData = Gf_DTSet(M_CN1, "D")
     End If
     If SDT_PROD_DATE_TO.RawData = "" Then
        SDT_PROD_DATE_TO.RawData = Gf_DTSet(M_CN1, "D")
     End If
End Sub

Private Sub SDT_PAINT_TO_DATE_GotFocus()
     If SDT_PROD_DATE_TO.RawData = "" Then
        SDT_PROD_DATE_TO.RawData = Gf_DTSet(M_CN1, "D")
     End If
End Sub

Private Sub Sum_Wgt()

    Dim iRow          As Integer
    
    Dim Surf_Wgt      As Double
    Dim Ust_Wgt       As Double
    Dim Quality_Wgt   As Double
    Dim iSumCol       As Integer
    
    ReDim sTotal(SS1_SURF_WGT To SS1_TOTAL_WGT)

    With ss1
           For iRow = 1 To .MaxRows
               .Row = iRow:

               .Col = SS1_SURF_WGT:      Surf_Wgt = Trim(Val(.Text & ""))
               .Col = SS1_UST_WGT:       Ust_Wgt = Trim(Val(.Text & ""))
               .Col = SS1_QUALITY_WGT:   Quality_Wgt = Trim(Val(.Text & ""))
               .Col = SS1_TOTAL_WGT:    .Text = Surf_Wgt + Ust_Wgt + Quality_Wgt
                                        .Text = Trim(Val(.Text & ""))
               
                For iSumCol = SS1_SURF_WGT To SS1_TOTAL_WGT
                    .Row = iRow
                    .Col = iSumCol
                    sTotal(iSumCol) = sTotal(iSumCol) + Val(.Text & "")
                Next iSumCol
            Next iRow
            
            .MaxRows = .MaxRows + 1
            For iSumCol = SS1_SURF_WGT To SS1_TOTAL_WGT
                .Row = .MaxRows
                .Col = SS1_ORD_NO:      .Text = "合计"
                .Col = iSumCol:         .Text = sTotal(iSumCol)
            Next iSumCol
    End With

End Sub

