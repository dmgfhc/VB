Public Function Gf_ComboAdd1(Conn As ADODB.Connection, Cbo As Variant, sQuery As String, Optional ClsChk As Boolean = True) As Boolean

On Error GoTo ComboAdd_Error

    Dim AdoRs As ADODB.Recordset
    Dim sQuery1 As String
    Dim sQuery2 As String
    Dim sQuery3 As String
     
    sQuery1 = "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS <>'DL' ORDER BY ROLL_NO"
'    sQuery1 = "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS = 'UI' UNION ALL "
'    sQuery1 = sQuery1 + "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS = 'GR' UNION ALL "
'    sQuery1 = sQuery1 + "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS = 'IN' UNION ALL "
'    sQuery1 = sQuery1 + "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS = 'UO' UNION ALL "
'    sQuery1 = sQuery1 + "SELECT ROLL_NO FROM GP_ROLL WHERE ROLL_STATUS = 'DL' "
    sQuery2 = "SELECT CHOCK_ID FROM GP_CHOCK WHERE STATUS<>'DL' OR STATUS IS NULL   "
    sQuery3 = "SELECT BEARING_ID FROM GP_BEARING WHERE STATUS<>'DL' OR STATUS IS NULL "
    'Db Connection Check
    If Conn Is Nothing Then
        If GF_DbConnect = False Then Gf_ComboAdd1 = False: Exit Function
    End If
    
    If ClsChk Then
        CBO_ROLL_ID.Clear
        CBO_PIAR_ROLL_ID.Clear
        CBO_WK_CHOCK.Clear
        CBO_DS_CHOCK.Clear
        CBO_PIAR_WK_CHOCK.Clear
        CBO_PIAR_DS_CHOCK.Clear
        CBO_WK_BEARING1.Clear
        CBO_WK_BEARING2.Clear
        CBO_WK_BEARING3.Clear
        CBO_WK_BEARING4.Clear
        CBO_DS_BEARING1.Clear
        CBO_DS_BEARING2.Clear
        CBO_DS_BEARING3.Clear
        CBO_DS_BEARING4.Clear
        CBO_PIAR_WK_BEARING1.Clear
        CBO_PIAR_WK_BEARING2.Clear
        CBO_PIAR_WK_BEARING3.Clear
        CBO_PIAR_WK_BEARING4.Clear
        CBO_PIAR_DS_BEARING1.Clear
        CBO_PIAR_DS_BEARING2.Clear
        CBO_PIAR_DS_BEARING3.Clear
        CBO_PIAR_DS_BEARING4.Clear
        
    End If
    
    Set AdoRs = New ADODB.Recordset

    'Ado Execute
    AdoRs.Open sQuery1, Conn, adOpenKeyset
    
    If Not AdoRs.BOF And Not AdoRs.EOF Then
        While Not AdoRs.EOF
            
            If VarType(AdoRs.Fields(0)) <> vbNull Then
                CBO_ROLL_ID.AddItem AdoRs.Fields(0)
                CBO_PIAR_ROLL_ID.AddItem AdoRs.Fields(0)
            End If
            AdoRs.MoveNext
            
        Wend
        Gf_ComboAdd1 = True
    Else
        Gf_ComboAdd1 = False
    End If
    
    AdoRs.Close
    
    AdoRs.Open sQuery2, Conn, adOpenKeyset
    
    If Not AdoRs.BOF And Not AdoRs.EOF Then
        While Not AdoRs.EOF
            
            If VarType(AdoRs.Fields(0)) <> vbNull Then
                CBO_WK_CHOCK.AddItem AdoRs.Fields(0)
                CBO_DS_CHOCK.AddItem AdoRs.Fields(0)
                CBO_PIAR_WK_CHOCK.AddItem AdoRs.Fields(0)
                CBO_PIAR_DS_CHOCK.AddItem AdoRs.Fields(0)
            End If
            AdoRs.MoveNext
            
        Wend
        Gf_ComboAdd1 = True
    Else
        Gf_ComboAdd1 = False
    End If
    
    AdoRs.Close
    
    AdoRs.Open sQuery3, Conn, adOpenKeyset
    
    If Not AdoRs.BOF And Not AdoRs.EOF Then
        While Not AdoRs.EOF
            
            If VarType(AdoRs.Fields(0)) <> vbNull Then
                CBO_WK_BEARING1.AddItem AdoRs.Fields(0)
                CBO_WK_BEARING2.AddItem AdoRs.Fields(0)
                CBO_WK_BEARING3.AddItem AdoRs.Fields(0)
                CBO_WK_BEARING4.AddItem AdoRs.Fields(0)
                CBO_DS_BEARING1.AddItem AdoRs.Fields(0)
                CBO_DS_BEARING2.AddItem AdoRs.Fields(0)
                CBO_DS_BEARING3.AddItem AdoRs.Fields(0)
                CBO_DS_BEARING4.AddItem AdoRs.Fields(0)
                CBO_PIAR_WK_BEARING1.AddItem AdoRs.Fields(0)
                CBO_PIAR_WK_BEARING2.AddItem AdoRs.Fields(0)
                CBO_PIAR_WK_BEARING3.AddItem AdoRs.Fields(0)
                CBO_PIAR_WK_BEARING4.AddItem AdoRs.Fields(0)
                CBO_PIAR_DS_BEARING1.AddItem AdoRs.Fields(0)
                CBO_PIAR_DS_BEARING2.AddItem AdoRs.Fields(0)
                CBO_PIAR_DS_BEARING3.AddItem AdoRs.Fields(0)
                CBO_PIAR_DS_BEARING4.AddItem AdoRs.Fields(0)
            End If
            AdoRs.MoveNext
            
        Wend
        Gf_ComboAdd1 = True
    Else
        Gf_ComboAdd1 = False
    End If
    
    AdoRs.Close
    Set AdoRs = Nothing
    
    Exit Function

ComboAdd_Error:

    Set AdoRs = Nothing
    Gf_ComboAdd1 = False

End Function
