Public Sub Form_Pro()

Dim sMesg As String
    
    TXT_ROLL_IN_EMP = sUserID

    If Not Gp_DateCheck(TXT_MILL_STA_TIME) Then
        sMesg = " 请正确输入装辊时间 ！"
        Call Gp_MsgBoxDisplay(sMesg)
        Exit Sub
    End If
    
    If Mid(CBO_ROLL_ID, 2, 1) = "1" Then
       If Mid(CBO_ROLL_TOP_BOT, 1, 1) <> "O" And Mid(CBO_ROLL_TOP_BOT, 1, 1) <> "D" Then
