Public Sub Spread_Can()

    Dim iRow As Integer
    Dim sTemp As String
    
    Call Gp_Sp_Cancel(M_CN1, Proc_Sc("Sc"))
    
    For iRow = 1 To ss1.MaxRows
                
        ss1.Row = iRow
        ss1.Col = 3
        sTemp = ss1.Text
        ss1.Col = 4
        
        If sTemp = "00" And ss1.Text = "00" Then
            Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, iRow, iRow, RED)
        ElseIf sTemp <> "00" And ss1.Text = "00" Then
            Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, iRow, iRow, BLUE)
        Else
            Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, iRow, iRow)
            ss1.Col = 11
            If Trim(ss1.Text) <> "定尺" Then
                ss1.Col = 12:    ss1.Lock = False
                Call Gp_Sp_CellColor(ss1, 12, iRow, BLACK, &HC0FFFF)
            Else
                ss1.Col = 12:    ss1.Lock = True
            End If
        End If
        
    Next iRow
      
End Sub

