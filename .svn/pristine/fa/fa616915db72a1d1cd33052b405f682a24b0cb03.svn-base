Private Sub cmd_move_Click()

On Error GoTo Move_Plate_ERROR

    Dim OutParam(2, 4) As Variant
    Dim ret_Result_ErrMsg As String
    Dim sQuery As String
    Dim iRow As Integer
    Dim sCur_Cut_Seq, sCur_Blc_Seq, sCur_Seq As String
    Dim sBef_Cut_Seq, sBef_Blc_Seq, sBef_Seq As String
    
    Dim adoCmd As ADODB.Command
    
    For iRow = 1 To ss1.MaxRows
    
        ss1.Row = iRow
        ss1.Col = 0
        
        If ss1.Text = "移动" Then
            ss1.Col = 2
            sCur_Cut_Seq = ss1.Text
            ss1.Col = 3
            sCur_Blc_Seq = ss1.Text
            ss1.Col = 4
            sCur_Seq = ss1.Text
            
            ss1.Row = iRow - 1
            ss1.Col = 2
            sBef_Cut_Seq = ss1.Text
            ss1.Col = 3
            sBef_Blc_Seq = ss1.Text
            ss1.Col = 4
            sBef_Seq = ss1.Text
            Exit For
        End If
    Next iRow
    
    If sCur_Cut_Seq = "" Or sCur_Blc_Seq = "" Or sCur_Seq = "" Or sBef_Cut_Seq = "" Or sBef_Blc_Seq = "" Or sBef_Seq = "" Then Exit Sub
    
    Screen.MousePointer = vbHourglass
    
    'Return Error Code Parameter
    OutParam(1, 1) = "arg_e_code"
    OutParam(1, 2) = adVarChar
    OutParam(1, 3) = adParamOutput
    OutParam(1, 4) = 1

    'Return Error Messsage Parameter
    OutParam(2, 1) = "arg_e_msg"
    OutParam(2, 2) = adVarChar
    OutParam(2, 3) = adParamOutput
    OutParam(2, 4) = 256
    
    sQuery = "{call CEC2080C.P_MOVE_PLATE (" & sdb_slab_edt_seq & ",'" + sCur_Cut_Seq + "','" + sCur_Blc_Seq + "','" + sCur_Seq + "','" + _
                                                                         sBef_Cut_Seq + "','" + sBef_Blc_Seq + "','" + sBef_Seq + "',?,?)} "
    
    'Ado Setting
    M_CN1.CursorLocation = adUseServer
    Set adoCmd = New ADODB.Command
    
    adoCmd.CommandType = adCmdText
    Set adoCmd.ActiveConnection = M_CN1
    
    adoCmd.CommandText = sQuery
    
    adoCmd.Parameters.Append adoCmd.CreateParameter(OutParam(1, 1), OutParam(1, 2), OutParam(1, 3), OutParam(1, 4))
    adoCmd.Parameters.Append adoCmd.CreateParameter(OutParam(2, 1), OutParam(2, 2), OutParam(2, 3), OutParam(2, 4))
    
    adoCmd.Execute , , adExecuteNoRecords
    
    'Process Error Check
    If adoCmd("arg_e_code") <> "" Then
        ret_Result_ErrMsg = adoCmd("arg_e_msg")
        sErrMessg = "Error Mesg : " & ret_Result_ErrMsg
        Call Gp_MsgBoxDisplay(sErrMessg)
        Set adoCmd = Nothing
        Screen.MousePointer = vbDefault
        Exit Sub
    Else
        Call Form_Ref
        bMoveFl = False
    End If
    
    Set adoCmd = Nothing
    Screen.MousePointer = vbDefault
    Exit Sub

Move_Plate_ERROR:

    Set adoCmd = Nothing
    Screen.MousePointer = vbDefault
    Call Gp_MsgBoxDisplay("Move_Plate_ERROR : " & Error)
    
End Sub

Private Sub cmd_move_down_Click()

    If ss1.SelBlockRow = ss1.MaxRows Then Exit Sub
    
    ss1.Row = ss1.ActiveRow
    
    ss1.Col = 0
    If ss1.Text <> "移动" And bMoveFl Then Exit Sub
    
    ss1.Col = 4
    If ss1.Text = "00" Then Exit Sub
    
    ss1.Col = 8
    If ss1.Text = "*" Then Exit Sub
    
    ss1.Col = 0
    ss1.Text = "移动"
    ss1.SwapRowRange ss1.SelBlockRow, ss1.SelBlockRow2, ss1.SelBlockRow + 1
    ss1.SetActiveCell 1, ss1.ActiveRow + 1
    
    bMoveFl = True

End Sub

Private Sub cmd_move_up_Click()

    If ss1.SelBlockRow <= 3 Then Exit Sub
    
    ss1.Row = ss1.ActiveRow
    
    ss1.Col = 0
    If ss1.Text <> "移动" And bMoveFl Then Exit Sub
    
    ss1.Col = 4
    If ss1.Text = "00" Then Exit Sub
    
    ss1.Col = 8
    If ss1.Text = "*" Then Exit Sub
    
    ss1.Col = 0
    ss1.Text = "移动"
    ss1.SwapRowRange ss1.SelBlockRow, ss1.SelBlockRow2, ss1.SelBlockRow - 1
    ss1.SetActiveCell 1, ss1.ActiveRow - 1
    
    bMoveFl = True
    
End Sub

