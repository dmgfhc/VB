Private Sub ss3_Click(ByVal Col As Long, ByVal Row As Long)

    'Call Gp_Sp_Sort(Proc_Sc("Sc")("Spread"), Col, Row)
    
    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If Row = 0 Then Exit Sub
    
    If ss3.MaxRows < 1 Then Exit Sub
    
    ss3.Row = Row
    ss3.Col = 0
    
    If ss3.Text <> "Update" Then
    
        'REM_WGT
        ss3.Col = 11
        If sdb_mat_wgt.Value + ss3.Value > sdb_ord_wgt.Value Then
        
            If Gf_MessConfirm("订单重量 < 材料重量", "Q") Then
            Else
                Exit Sub
            End If
        
        End If
        
        ss3.Col = 0
        ss3.Text = "Update"
        
        ss3.Col = 13
        ss3.Text = txt_ord_no1.Text
        ss3.Col = 14
        ss3.Text = txt_ord_item1.Text
        ss3.Col = 15
        ss3.Text = txt_prod_cd1.Text
        ss3.Col = 16
        ss3.Text = sUserID
        
        'REM_WGT
        ss3.Col = 11
        sdb_mat_wgt.Value = sdb_mat_wgt.Value + ss3.Value
        
        Call Gp_Sp_BlockColor(ss3, 1, ss3.MaxCols, Row, Row, , &HFFFF80)
    Else
        ss3.Text = ""
        
        ss3.Col = 13
        ss3.Text = ""
        ss3.Col = 14
        ss3.Text = ""
        ss3.Col = 15
        ss3.Text = ""
        ss3.Col = 16
        ss3.Text = ""
        
        'REM_WGT
        ss3.Col = 11
        sdb_mat_wgt.Value = sdb_mat_wgt.Value - ss3.Value
        
        Call Gp_Sp_BlockColor(ss3, 1, ss3.MaxCols, Row, Row)
    End If
    
