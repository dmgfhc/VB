Private Sub ss1_Click(ByVal Col As Long, ByVal Row As Long)
    
    'Call Gp_Sp_Sort(Proc_Sc("Sc")("Spread"), Col, Row)
    
    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If Row = 0 Then Exit Sub
    
    If ss1.MaxRows < 1 Then Exit Sub
    
    ss1.Row = Row
    ss1.Col = 0
    
    If ss1.Text <> "选择" Then
    
        If iCss1_Row > 0 Then Exit Sub
        
        ss1.Text = "选择"
        Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row, , &HFFFF80)
        
        'ORD_NO
        ss1.Col = 1
        txt_ord_no1.Text = ss1.Text
        
        'ORD_ITEM
        ss1.Col = 2
        txt_ord_item1.Text = ss1.Text
        
        'PROD_CD
        ss1.Col = 6
        txt_prod_cd1.Text = ss1.Text
        
        'STLGRD
        ss1.Col = 7
        txt_stlgrd1.Text = ss1.Text
        
        'THK
        ss1.Col = 8
        sdb_thk1.Value = ss1.Value
        
        'WID
        ss1.Col = 9
        sdb_wid1.Value = ss1.Value
        
        'LEN
        ss1.Col = 10
        sdb_len1.Value = ss1.Value
                
        'WGT
        'ss1.Col = 10
        'sdb_len1.Value = ss1.Value
        
        'REM_WGT
        ss1.Col = 14
        sdb_ord_wgt.Value = ss1.Value
        
        iCss1_Row = Row
        
        Call Gf_Sp_Refer(M_CN1, Sc2, Mc1, False)
        
    Else
    
        If Gf_Sp_Cls(Sc3) Then
            If Gf_Sp_Cls(Sc2) Then
            
                ss1.Text = ""
                Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row)
                
                Call Gp_Ms_Cls(Mc2("rControl"))
    
                sdb_ord_wgt.Value = 0
                sdb_mat_wgt.Value = 0
                iCss1_Row = 0
                iCss2_Row = 0
                iCss3_Row = 0
                
                txt_ord_no1.Text = ""
                txt_ord_item1.Text = ""
                txt_ord_no2.Text = ""
                txt_ord_item2.Text = ""
                txt_prod_cd1.Text = ""
                txt_stlgrd1.Text = ""
                sdb_thk1.Value = 0
                sdb_wid1.Value = 0
                sdb_len1.Value = 0
                
            End If
        End If
        
    End If

