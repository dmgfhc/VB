     '   ss1.Col = 5
     '   txt_cust_cd_fr.Text = ss1.Value
        ss1.Col = 17
        sdb_ord_wgt = ss1.Text
        
        
        iCss1_Row = Row
        
        Call Gf_Sp_Refer(M_CN1, Sc2, Mc2, Mc2("nControl"), Mc2("mControl"), False)
        ss2.OperationMode = OperationModeNormal
        
    Else
    
        ss1.Text = ""
        Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row)
        
        Call Gf_Sp_Cls(Sc2)
        
        txt_ord_no1.Text = ""
        txt_ord_item1.Text = ""
        
        sdb_ord_wgt.Value = 0
        sdb_mat_wgt.Value = 0
        iCss1_Row = 0
        iCss2_Row = 0
        
    End If
                
