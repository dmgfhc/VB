        ss2.Text = sUserID
        
        'REM_WGT
        ss2.Col = 16
        sdb_mat_wgt.Value = sdb_mat_wgt.Value + ss2.Value
        
        Call Gp_Sp_BlockColor(ss2, 1, ss2.MaxCols, Row, Row, , &HFFFF80)
        
    Else
        ss2.Text = ""
        
        ss2.Col = 18
        ss2.Text = ""
        ss2.Col = 19
        ss2.Text = ""
        ss2.Col = 20
        ss2.Text = ""
        ss2.Col = 21
        ss2.Text = ""
        
        'REM_WGT
        ss2.Col = 16
        sdb_mat_wgt.Value = sdb_mat_wgt.Value - ss2.Value
        
        Call Gp_Sp_BlockColor(ss2, 1, ss2.MaxCols, Row, Row)
    End If
    
End Sub

