    Call Gp_Sp_ReadOnlySet(Sc2.Item("Spread"))
    
    Call Gf_Sp_Cls(sc1)
    Call Gf_Sp_Cls(Sc2)
    
    Call Gp_Sp_ColGet(sc1.Item("Spread"), "C-System.INI", Me.Name)
    Call Gp_Sp_ColGet(Sc2.Item("Spread"), "C-System.INI", Me.Name)
    
