Private Sub ss1_Click(ByVal Col As Long, ByVal Row As Long)

    Dim sQuery As String
    
    Call Gp_Sp_Sort(sc1.Item("Spread"), Col, Row)
    Set Active_Spread = Me.ss1
    
    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If ss1.MaxRows < 1 Or Row < 1 Then Exit Sub
    
    ss1.Row = Row
    ss1.Col = 0
    
    If ss1.Text = "" Then
        If Slab_Cnt <> 0 Then Exit Sub
        ss1.Row = Row
        
        If Select_SlabCnt > 0 Then
            ss1.Col = 3
            If Select_Slab_Stlgrd <> ss1.Text Then Exit Sub
            
            ss1.Col = 7
            If Select_Slab_Thk <> ss1.Value Then Exit Sub
            
            ss1.Col = 8
            If Select_Slab_Wid <> ss1.Value Then Exit Sub
            
            ss1.Col = 9
            If Select_Slab_Len <> ss1.Value Then Exit Sub
            
            ss1.Col = 17
            If Select_Owner_Fl <> ss1.Text Then Exit Sub
            
            ss1.Col = 18
            If Select_Owner_Cust <> ss1.Text Then Exit Sub
            
        End If
        
        ss1.Col = 0:    ss1.Text = "替代"
        Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row, , &HFFFF80)
        ss1.Col = 1:    sSlab_No = ss1.Text
        Select_SlabCnt = Select_SlabCnt + 1
        
    Else
    
        If Slab_Cnt <> 0 Then Exit Sub
        ss1.Col = 0:    ss1.Text = ""
        Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row)
        Select_SlabCnt = Select_SlabCnt - 1
        
        If Select_SlabCnt = 0 Then
            Call Gf_Sp_Cls(sc2)
            sdb_mat_wgt.Value = 0
            sdb_div_wgt.Value = 0
            sdb_cut_wgt.Value = 0
            sdb_ord_wgt.Value = 0
            txt_stlgrd1.Text = ""
            txt_stdspec1.Text = ""
            sdb_slab_thk1.Value = 0
            sdb_slab_wid1.Value = 0
            sdb_slab_wgt1.Value = 0
            txt_owner_fl.Text = ""
            txt_owner_cust.Text = ""
            sSlab_No = ""
        End If
        Exit Sub
    End If
  
    If Select_SlabCnt = 1 Then
    
        lCurrRow = Row
        ss1.Row = Row
        
        ss1.Col = 3
        txt_stlgrd1.Text = ss1.Text
        Select_Slab_Stlgrd = ss1.Text
        
        ss1.Col = 5
        txt_stdspec1.Text = ss1.Text
        
        ss1.Col = 7
        sdb_slab_thk1.Value = ss1.Value
        Select_Slab_Thk = ss1.Value
        
        ss1.Col = 8
        sdb_slab_wid1.Value = ss1.Value
        Select_Slab_Wid = ss1.Value
        
        ss1.Col = 9
        Select_Slab_Len = ss1.Value
        
        ss1.Col = 10
        sdb_mat_wgt.Value = ss1.Value
        sdb_div_wgt.Value = ss1.Value
        
        sQuery = "SELECT GF_JP_WGT('WGT','" & Select_Slab_Stlgrd & "', " & Select_Slab_Thk & ", " & Select_Slab_Wid & ", " & dSlab_Cut_Spare_Len & ",0) FROM DUAL "
        dSlab_Cut_Spare_Wgt = Gf_FloatFind(M_CN1, sQuery)
        
        sdb_cut_wgt.Value = (ss1.Value - (cbo_slab_cut.ListIndex) * dSlab_Cut_Spare_Wgt) / (cbo_slab_cut.ListIndex + 1)
        sdb_slab_wgt1.Value = sdb_cut_wgt.Value
        
        ss1.Col = 17
        txt_owner_fl.Text = IIf(ss1.Text <> "", "B", "")
        Select_Owner_Fl = ss1.Text
        
        ss1.Col = 18
        txt_owner_cust.Text = ss1.Text
        Select_Owner_Cust = ss1.Text
        
        Slab_Cnt = 0
        
        Call Gf_Sp_Refer(M_CN1, sc2, Mc2, , , False)
        ss2.OperationMode = OperationModeNormal
        
        Call Gp_Sp_ColColor(ss2, 11, BLUE)
        Call Gp_Sp_ColColor(ss2, 12, RED, &HC0FFFF)
        
        Call Gp_Sp_ColColor(ss2, 19, BLUE)
        Call Gp_Sp_ColColor(ss2, 20, RED)
        
        Call Gp_Sp_ColColor(ss2, 21, BLUE)
        Call Gp_Sp_ColColor(ss2, 22, RED)
        
    End If
    
End Sub

