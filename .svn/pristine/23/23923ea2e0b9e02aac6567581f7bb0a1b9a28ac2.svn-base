Private Sub ss2_Click(ByVal Col As Long, ByVal Row As Long)
    
    Dim sQuery As String
    Dim lRow As Integer
    Dim Prod_Cnt As Integer
    Dim Slab_uWgt As Double
    Dim Slab_Rem_Wgt As Double
    Dim Slab_dLen As Double
    Dim sOrd_No As String
    Dim sOrd_item As String
    Dim dWid_Margin As Double
    Dim dProd_Thk As Double
    Dim dProd_Wid As Double
    Dim dAsroll_Thk As Double
    Dim dAsroll_Wid As Double
    Dim dChg_Prod_Len As Double
    Dim dProd_Wgt As Double
    Dim sTrim_Fl As String
    Dim sProd_Cd As String
    Dim sStlgrd As String
    
    Call Gp_Sp_Sort(sc2.Item("Spread"), Col, Row)
    
    Set Active_Spread = Me.ss2
    
    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If ss2.MaxRows < 1 Or Row < 1 Then Exit Sub
    If Col <> 0 Then Exit Sub
    
    ss2.Row = Row
    ss2.Col = 8
    sProd_Cd = ss2.Text
    ss2.Col = 9
    dProd_Thk = ss2.Value
    ss2.Col = 10
    dProd_Wid = ss2.Value
    ss2.Col = 27
    sTrim_Fl = ss2.Text
    
    'Select Order Wid Margin
    sQuery = "SELECT NVL(WID_MARGIN,0) FROM NISCO.EP_PLATEWID_STD "
    sQuery = sQuery & " WHERE  PLT         =  'C3' "
    sQuery = sQuery & "   AND  PROD_CD     =  '" & sProd_Cd & "' "
    sQuery = sQuery & "   AND  TRIM_FL     =  '" & sTrim_Fl & "' "
    sQuery = sQuery & "   AND  THK_MIN     <=  " & dProd_Thk
    sQuery = sQuery & "   AND  THK_MAX     >   " & dProd_Thk
    sQuery = sQuery & "   AND  WID_MIN     <=  " & dProd_Wid
    sQuery = sQuery & "   AND  WID_MAX     >   " & dProd_Wid
    
    dWid_Margin = Gf_FloatFind(M_CN1, sQuery)
    dWid_Margin = dWid_Margin + dProd_Wid
    
    If Select_Slab_Wid > dWid_Margin And Select_Slab_Len > dWid_Margin Then
        If Not Gf_MessConfirm("已超过产品宽度标准,确定继续吗?", "I") Then
            Exit Sub
        End If
    End If
    
    ss2.Row = Row
    ss2.Col = 1
    sOrd_No = ss2.Text
    
    ss2.Col = 2
    sOrd_item = ss2.Text
    
    ss2.Col = 23
    Prod_Cnt = ss2.Value
    
    ss2.Col = 22
    Slab_uWgt = ss2.Value
    
    If sdb_ord_wgt.Value + Slab_uWgt > (sdb_mat_wgt.Value + 0.01) Then
        Call Gp_MsgBoxDisplay("材料重量 < 替代重量", "I", Me.Caption)
        Exit Sub
    End If
    
'    ss2.Col = 32
'    If Slab_uWgt > ss2.Value Then
'        Call Gp_MsgBoxDisplay("板坯重量 > 替代剩余量", "I", Me.Caption)
'        Exit Sub
'    End If
    
    '替代剩余产品数
    ss2.Col = 34
    ss2.Value = ss2.Value + (Prod_Cnt * Select_SlabCnt)
    
    '替代产品数
    ss2.Col = 35
    ss2.Value = ss2.Value - (Prod_Cnt * Select_SlabCnt)
    
    Slab_Cnt = Slab_Cnt + 1
    
    If Slab_Cnt = 1 Then
        sdb_ord_wgt.Value = sdb_ord_wgt.Value + Slab_uWgt
        sdb_div_wgt.Value = sdb_div_wgt.Value - Slab_uWgt
    Else
        sdb_ord_wgt.Value = sdb_ord_wgt.Value + (Slab_uWgt + dSlab_Cut_Spare_Wgt)
        sdb_div_wgt.Value = sdb_div_wgt.Value - (Slab_uWgt + dSlab_Cut_Spare_Wgt)
    End If
    
    If Slab_Cnt > 0 Then cbo_slab_cut.Enabled = False
    
    Load lbl_slab(Slab_Cnt)
    lbl_slab(Slab_Cnt).Tag = Right("0" & Str(Slab_Cnt), 2)
    lbl_slab(Slab_Cnt).Caption = Right("0" & Str(Slab_Cnt), 2)
    lbl_slab(Slab_Cnt).Top = 200
    lbl_slab(Slab_Cnt).Height = 555
    
    'If sdb_div_wgt.Value < 0 Then
    '    lbl_slab(Slab_Cnt).Width = (Shape1.Width / sdb_mat_wgt.Value) * (Slab_uWgt + sdb_div_wgt.Value + IIf(Slab_Cnt > 0, dSlab_Cut_Spare_Wgt, 0))
    'Else
        lbl_slab(Slab_Cnt).Width = (Shape1.Width / sdb_mat_wgt.Value) * (Slab_uWgt + IIf(Slab_Cnt > 0, dSlab_Cut_Spare_Wgt, 0))
    'End If
    
    If Slab_Cnt = 1 Then
        lbl_slab(Slab_Cnt).Left = Shape1.Left
    Else
