Private Sub Form_Load()

    Screen.MousePointer = vbHourglass
    
    sAuthority = Gf_Pgm_Authority(Me.Name)
    
    Call Form_Define
    
    cbo_shift.AddItem ""
    cbo_shift.AddItem "1"
    cbo_shift.AddItem "2"
    cbo_shift.AddItem "3"
    
    cbo_group.AddItem ""
    cbo_group.AddItem "A"
    cbo_group.AddItem "B"
    cbo_group.AddItem "C"
    cbo_group.AddItem "D"
    
    cbo_prc_line.AddItem "1"
    cbo_prc_line.AddItem "2"
    cbo_prc_line.AddItem "3"

    
    Call Gp_Ms_Cls(Mc1("rControl"))
    Call Gp_Ms_NeceColor(Mc1("nControl"))
    
    Call MDIMain.FormMenuSetting(Me, FormType, "FS", sAuthority)
    
    Call Gp_Sp_Setting(Proc_Sc("Sc")("Spread"), False)
 

    Call Gf_Sp_Cls(Proc_Sc("Sc"))
    
    Call Gp_Sp_ColGet(Proc_Sc("Sc")("Spread"), "F-System.INI", Me.Name)
    
    With ss1
    
        .Row = 0: .Row2 = 0
        .Col = 66: .Col2 = 66
        
        .BlockMode = True
        
        .CellType = SS_CELL_TYPE_STATIC_TEXT
        .TypeHAlign = SS_CELL_H_ALIGN_CENTER
        .TypeVAlign = SS_CELL_V_ALIGN_CENTER
'        .TypeTextWordWrap = True
        
        .BackColor = &HE1E4CD
        .ForeColor = RED
        
        .BlockMode = False
        
    End With
        
    txt_from_DATE.RawData = Gf_DTSet(M_CN1, "D")
    txt_to_DATE.RawData = Gf_DTSet(M_CN1, "D")
        
    Screen.MousePointer = vbDefault

End Sub

