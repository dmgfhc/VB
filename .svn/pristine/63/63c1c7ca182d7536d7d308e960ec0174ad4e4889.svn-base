Public Sub Form_Cls()
    
    If Gf_Sp_Cls(sc1) Then
        If Gf_Sp_Cls(sc2) Then
            Call Gp_Ms_Cls(Mc1("rControl"))
            Call Gp_Ms_Cls(Mc2("rControl"))
            Call MDIMain.FormMenuSetting(Me, FormType, "CLS", sAuthority)
            Call MenuTool_ReSet
            rContro1(1).SetFocus
            iCurr_Row1 = 0
            iCurr_Row2 = 0
            
            If App.Title = "AC" Then
                txt_plt.Text = "C1"
            ElseIf App.Title = "BG" Then
                txt_plt.Text = "C1"
            ElseIf App.Title = "CG" Then
                txt_plt.Text = "C3"
            End If
        
            Call txt_plt_KeyUp(0, 0)
            
            dpt_del_fr.RawData = ""
            dpt_del_to.RawData = ""
        End If
    End If
    
End Sub

Public Sub Form_Ref()

    Dim lRow As Integer
    
    If Gf_Sp_ProceExist(sc2.Item("Spread")) Then Exit Sub
    
    If Gf_Sp_Refer(M_CN1, sc2, Mc1) Then
        ss2.OperationMode = OperationModeNormal
        'Call Gp_Sp_EvenRowBackcolor(ss2)
        Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
        Call MenuTool_ReSet
        Call Gf_Sp_Cls(sc1)
        Call Gp_Ms_Cls(Mc2("rControl"))
        iCurr_Row1 = 0
        iCurr_Row2 = 0
        
        For lRow = 1 To ss2.MaxRows
        
            ss2.Row = lRow
            ss2.Col = ss2.MaxCols
            
            If ss2.Value < 0 Then
                Call Gp_Sp_RowColor(ss2, lRow, &HFF&)
            End If
        
        Next lRow
        
    End If
            
End Sub

Public Sub Form_Pro()

    Dim lRow As Integer
    
    If txt_woo_rsn.Text = "" Or txt_woo_rsn_nm.Text = "" Then
        Call Gp_MsgBoxDisplay("余材原因必须输入", "I")
        Exit Sub
    End If
    
    For lRow = 1 To ss1.MaxRows
    
        ss1.Row = lRow
        ss1.Col = 0
        
        If ss1.Text = "Update" Then
            ss1.Col = ss1.MaxCols - 1
            ss1.Text = txt_woo_rsn.Text
        End If
    
    Next lRow
    
    If Gf_Sp_Process(M_CN1, sc1, Mc2, False) Then
        ss1.OperationMode = OperationModeNormal
        Call Gp_Sp_EvenRowBackcolor(ss1)
        Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
        Call MenuTool_ReSet
        
        If iCurr_Row2 <> 0 Then
            Call Gp_Sp_OneRowDisplay(M_CN1, Gf_Sp_MakeQuery(sc2.Item("Spread"), sc2.Item("P-O"), "O", sc2.Item("pColumn"), iCurr_Row2), sc2.Item("Spread"), iCurr_Row2)
        End If
    End If
    
End Sub

