Private Sub ss1_Click(ByVal Col As Long, ByVal Row As Long)

    'Call Gp_Sp_Sort(Proc_Sc("Sc")("Spread"), Col, Row)
    
    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If ss1.MaxRows < 1 Or Row < 1 Then Exit Sub
    
    If txt_woo_rsn.Text = "" Or txt_woo_rsn_nm.Text = "" Then
        Call Gp_MsgBoxDisplay("余材原因必须输入", "I")
        Exit Sub
    End If
    
    ss1.Row = Row
    ss1.Col = 0
    
    If ss1.Text = "Update" Then
    
        ss1.Col = 0
        ss1.Text = ""
        ss1.Col = ss1.MaxCols - 2
        ss1.Text = ""
        ss1.Col = ss1.MaxCols - 1
        ss1.Text = ""
        ss1.Col = ss1.MaxCols
        ss1.Text = ""
        
        If Row Mod 2 <> 0 Then
            Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row, , &HF2F2F2)
        Else
            Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row)
        End If
        
    Else
    
        If iCurr_Row2 <> 0 Then
            ss2.Row = iCurr_Row2
            ss2.Col = 1
            txt_ord_no1.Text = ss2.Text
            ss2.Col = 2
            txt_ord_item1.Text = ss2.Text
            ss2.Col = ss2.MaxCols
        End If
        
        ss1.Row = Row
        ss1.Col = 0
        ss1.Text = "Update"
        ss1.Col = ss1.MaxCols - 1
        ss1.Text = txt_woo_rsn.Text
        ss1.Col = ss1.MaxCols - 3
        ss1.Text = txt_woo_rsn_nm.Text
        ss1.Col = ss1.MaxCols
        ss1.Text = sUserID
    
        Call Gp_Sp_BlockColor(ss1, 1, ss1.MaxCols, Row, Row, , &HFFFF80)
        
    End If
    
End Sub

Private Sub ss1_RightClick(ByVal ClickType As Integer, ByVal Col As Long, ByVal Row As Long, ByVal MouseX As Long, ByVal MouseY As Long)

    If Row > 0 Then
        Set Active_Spread = Me.ss1
        MDIMain.Mnu_Sorting.Enabled = False
        PopupMenu MDIMain.PopUp_Spread
        MDIMain.Mnu_Sorting.Enabled = True
    End If

End Sub

Private Sub ss2_BlockSelected(ByVal BlockCol As Long, ByVal BlockRow As Long, ByVal BlockCol2 As Long, ByVal BlockRow2 As Long)
    
    lBlkcol1 = BlockCol
    lBlkcol2 = BlockCol2
    lBlkrow1 = BlockRow
    lBlkrow2 = BlockRow2

End Sub

Private Sub ss2_Click(ByVal Col As Long, ByVal Row As Long)

    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0
    
    If ss2.MaxRows < 1 Or Row < 1 Then Exit Sub
    
    If iCurr_Row2 <> 0 Then
        
        ss2.Row = iCurr_Row2
        ss2.Col = ss2.MaxCols
        
        If ss2.Value < 0 Then
            Call Gp_Sp_RowColor(ss2, iCurr_Row2, &HFF&)
        Else
            Call Gp_Sp_RowColor(ss2, iCurr_Row2)
        End If
    
    End If
    
    Call Gp_Ms_Cls(Mc2("rControl"))
    
    ss2.Row = Row
    ss2.Col = 1
    txt_ord_no1.Text = ss2.Text
    ss2.Col = 2
    txt_ord_item1.Text = ss2.Text
    
    iCurr_Row2 = Row
    ss2.Row = Row
    ss2.Col = ss2.MaxCols
    
    If ss2.Value < 0 Then
        Call Gp_Sp_RowColor(ss2, iCurr_Row2, &HFF&, &HFFFF80)
    Else
        Call Gp_Sp_RowColor(ss2, iCurr_Row2, , &HFFFF80)
    End If
    
    Call Gf_Sp_Refer(M_CN1, sc1, Mc2, , , False)
    ss1.OperationMode = OperationModeNormal
    Call Gp_Sp_EvenRowBackcolor(ss1)
    iCurr_Row1 = 0
    
End Sub

Private Sub ss2_LostFocus()

    lBlkcol1 = 0
    lBlkcol2 = 0
    lBlkrow1 = 0
    lBlkrow2 = 0

End Sub

