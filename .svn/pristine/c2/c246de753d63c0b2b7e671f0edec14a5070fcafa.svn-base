Private Function Hmps_ComboAdd(Conn As ADODB.Connection, Cbo As Variant, sPrcLine As String, Optional ClsChk As Boolean = True) As Boolean

On Error GoTo Hmps_ComboAdd_Error
    
    Dim AdoRs  As ADODB.Recordset
    Dim sQuery As String
    
    'Db Connection Check
    If Conn Is Nothing Then
        If GF_DbConnect = False Then Hmps_ComboAdd = False: Exit Function
    End If
    
    If ClsChk Then
        Cbo.Clear
    End If
     
    sQuery = "         SELECT C.HEAT_NO "
    sQuery = sQuery & "  FROM ( "
    sQuery = sQuery & "          SELECT A.HEAT_NO "
    sQuery = sQuery & "            FROM FP_HMPS A "
    sQuery = sQuery & "           WHERE A.PRC_LINE   LIKE '" & Trim(sPrcLine) & "%'"
    sQuery = sQuery & "             AND HMPS_END_DATE IS NOT NULL "
    sQuery = sQuery & "           ORDER BY A.HMPS_END_DATE DESC ) C"
    sQuery = sQuery & "   WHERE ROWNUM <= 15 "
    sQuery = sQuery & "   ORDER BY C.HEAT_NO DESC "
    
    Set AdoRs = New ADODB.Recordset
     
    'Ado Execute
    AdoRs.Open sQuery, Conn, adOpenKeyset
     
    If Not AdoRs.BOF And Not AdoRs.EOF Then
        While Not AdoRs.EOF
            
            If VarType(AdoRs.Fields(0)) <> vbNull Then
               If AdoRs.Fields(0) > "0" Then
                  Cbo.AddItem AdoRs.Fields(0)
                  
               Else
                  Cbo.AddItem AdoRs.Fields(0)
              
               End If
            End If
            AdoRs.MoveNext
            
        Wend
        Hmps_ComboAdd = True
    Else
         Cbo.AddItem ""
        Hmps_ComboAdd = False
    End If
    
    AdoRs.Close
    Set AdoRs = Nothing
    
    Exit Function

Hmps_ComboAdd_Error:

    Set AdoRs = Nothing
    Hmps_ComboAdd = False

End Function

