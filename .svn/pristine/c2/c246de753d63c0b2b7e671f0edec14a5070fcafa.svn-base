Private Sub cbo_ld_id_Click()

    Dim sHeat_No As String
    Dim iHm_Charge_Wgt     As String
    Dim iHmps_Recv_Wgt     As String
    Dim iHm_Rem_Wgt     As Integer
    Dim iHeat_id           As String
    
    sHeat_No = Gf_CodeFind(M_CN1, "SELECT LD_CHARGE_NO FROM FP_LADLE WHERE LADLE_NO = '" & Trim(cbo_ld_id) & "'")
    iHeat_id = Gf_CodeFind(M_CN1, "SELECT MAX(A.HEAT_NO) FROM FP_CONRSLT A, FP_CONSTATUS B where A.HEAT_NO = B.HEAT_NO" + _
                                     " AND A.HEAT_NO LIKE SUBSTR(TO_CHAR(SYSDATE,'YYYYMMDD'),3,2) || '10%'" + _
                                     " AND B.CLADLE_NO = '" & Trim(cbo_ld_id) & "'" + _
                                     " AND B.STATUS = 9 ")
    If Len(iHeat_id) = 8 Then
      iHm_Charge_Wgt = Gf_CodeFind(M_CN1, "SELECT HM_NET_WGT FROM FP_CONRSLT WHERE HEAT_NO = '" + iHeat_id + "'")
    End If
    
    iHmps_Recv_Wgt = Gf_CodeFind(M_CN1, "SELECT HMPS_RECV_WGT FROM FP_LADLE WHERE LADLE_NO = '" & Trim(cbo_ld_id) & "'")
    
    If iHm_Charge_Wgt > "0" And iHmps_Recv_Wgt > "0" Then
