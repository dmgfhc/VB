Private Sub TreeView_Setting()

On Error GoTo Error_Rtn

    Dim AdoRs As New ADODB.Recordset
    Dim nodX As Node

    Set nodX = tre_view.Nodes.Add(, , "root", "南京钢铁", 1, 2)
    nodX.Image = 1
    
    'DEPARTMENT
    Set AdoRs = Gf_Ms_Rset(M_CN1, "SELECT CD, CD_NAME FROM ZP_CD WHERE CD_MANA_NO = 'Z0002' ORDER BY CD ASC")
    If Not AdoRs.EOF Then
        Do Until AdoRs.EOF Or AdoRs.BOF
            Set nodX = tre_view.Nodes.Add("root", tvwChild, "A" & Trim(AdoRs.Fields(0)), AdoRs.Fields(1), 3, 4)
            nodX.Image = 3
            AdoRs.MoveNext
        Loop
    
        'EMPLOYEED
        Set AdoRs = Gf_Ms_Rset(M_CN1, "SELECT DEPT, EMP_ID, EMP_NAME FROM ZP_EMPLOYEE ORDER BY DEPT ASC, EMP_ID ASC")
        If Not AdoRs.EOF Then
            Do Until AdoRs.EOF
                Set nodX = tre_view.Nodes.Add("A" & Trim(AdoRs.Fields(0)), tvwChild, "B" & AdoRs.Fields(1), AdoRs.Fields(1) & " " & AdoRs.Fields(2), 5, 6)
                nodX.Image = 5
                AdoRs.MoveNext
            Loop
        End If
    End If
    
    Exit Sub
    
Error_Rtn:
    Set AdoRs = Nothing

End Sub

Private Sub tre_view_Click()
    
    If Left(tre_view.SelectedItem.Key, 1) <> "B" Then Exit Sub
    txt_emp_id.Text = Trim(Mid(tre_view.SelectedItem.Key, 2, Len(Trim(tre_view.SelectedItem.Key)) - 1))
    txt_emp_name.Text = Trim(Mid(tre_view.SelectedItem.Text, 8, Len(Trim(tre_view.SelectedItem.Text)) - 7))
    
    Call Form_Ref
    
End Sub

Public Sub Add_Display(Conn As ADODB.Connection, sQuery As String)

On Error GoTo Error_Rtn
    
    Dim lRow As Long
    Dim lRowCount As Long
    Dim lCurrRow As Long
    
    Dim AdoRs As ADODB.Recordset
    Dim ArrayRecords As Variant
    
    'Db Connection Check
    If Conn Is Nothing Then
        If GF_DbConnect = False Then Exit Sub
    End If
    
    Set AdoRs = New ADODB.Recordset
        
    With ss1
    
        .ReDraw = False
        Screen.MousePointer = vbHourglass
        
        'Ado Execute
        AdoRs.Open sQuery, Conn, adOpenKeyset
        
        If AdoRs.BOF Or AdoRs.EOF Then
        
            .ReDraw = True
            AdoRs.Close
            Set AdoRs = Nothing
        
            Screen.MousePointer = vbDefault
            Exit Sub
            
        End If
        
        ArrayRecords = AdoRs.GetRows
        AdoRs.Close
        Set AdoRs = Nothing
        
        If UBound(ArrayRecords, 1) >= 0 Then
        
            lRowCount = UBound(ArrayRecords, 2) + 1
            lCurrRow = .MaxRows
            .MaxRows = .MaxRows + lRowCount
        
            For lRow = 1 To lRowCount
                
                .Row = lCurrRow + lRow
                
                .Col = 0:   .Text = "Input"
                
                .Col = 2
                If VarType(ArrayRecords(0, lRow - 1)) = vbNull Then
                    .Text = ""
                Else
                    .Text = Trim(ArrayRecords(0, lRow - 1))
                End If
                
                .Col = 3
                If VarType(ArrayRecords(1, lRow - 1)) = vbNull Then
                    .Text = ""
                Else
                    .Text = Trim(ArrayRecords(1, lRow - 1))
                End If
'                .Col = 4:   .Text = 1
                .Col = 10:  .Text = sUserID
                .Col = 11:  .Text = sUserName
            
            Next lRow
        
        End If
                                            
        .ReDraw = True
        
    End With
    
    Screen.MousePointer = vbDefault
    Exit Sub
   
Error_Rtn:

    Set AdoRs = Nothing
    Screen.MousePointer = vbDefault
    
End Sub

