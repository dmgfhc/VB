Public Sub Form_Ref()

    Dim sEdate1 As String
    sEdate1 = Mid(dtp_yy_mm.RawData, 1, 6)
    On Error GoTo Err_Track
                
    Set adoRs = New ADODB.Recordset
    
    sQuery = "{call AAA1091C.P_REFER1('" + sEdate1 + "')}"

        
    adoRs.Open sQuery, M_CN1, adOpenKeyset

    If adoRs.EOF Then GoTo Err_Track
       
    arrRecords1 = adoRs.GetRows
    adoRs.Close
'-----------------------------------------------------------------------------
    sQuery = "{call AAA1091C.P_REFER2('" + sEdate1 + "')}"

        
    adoRs.Open sQuery, M_CN1, adOpenKeyset

    If adoRs.EOF Then GoTo Err_Track
       
    arrRecords2 = adoRs.GetRows
    adoRs.Close
'-----------------------------------------------------------------------------
    sQuery = "{call AAA1091C.P_REFER3('" + sEdate1 + "')}"

    adoRs.Open sQuery, M_CN1, adOpenKeyset

    If adoRs.EOF Then GoTo Err_Track

    arrRecords3 = adoRs.GetRows
    adoRs.Close
    
'-----------------------------------------------------------------------------
    sQuery = "{call AAA1090C.P_REFER4('" + sEdate1 + "')}"

    adoRs.Open sQuery, M_CN1, adOpenKeyset

    If adoRs.EOF Then GoTo Err_Track

    arrRecords4 = adoRs.GetRows
    adoRs.Close
'-----------------------------------------------------------------------------
    Set adoRs = Nothing
    
    Call Sheet_1
    Call Sheet_2
    Call Sheet_3
    Call Sheet_4
    
    Exit Sub
    
Err_Track:
        
    If IsObject(adoRs) = True Then
        Set adoRs = Nothing
    End If
    
End Sub

