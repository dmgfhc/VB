Public Sub Sp_Header_display(sPname As Variant)

On Error GoTo SpreadDisplay_Error

    Dim iCol As Integer
    Dim iCnt As Integer
    Dim iColCnt As Integer
    Dim sQuery As String
    Dim sMesg As String
    Dim adoRs As ADODB.Recordset
    Dim ArrayRecords As Variant
    
    Set adoRs = New ADODB.Recordset
    
  
    sQuery = "Select Count(*) FROM QP_CHEM_SEQ"
    
    adoRs.Open sQuery, M_CN1, adOpenKeyset, , adCmdText
    
    If adoRs.BOF And adoRs.EOF Then
        GoTo SpreadDisplay_Error
    End If
    
    iColCnt = adoRs.Fields(0).Value
    adoRs.Close
    
    sQuery = "Select CHEM_COMP_CD FROM QP_CHEM_SEQ ORDER BY CHEM_COMP_SEQ"
                    
    adoRs.Open sQuery, M_CN1, adOpenKeyset
        
    If adoRs.BOF And adoRs.EOF Then
        GoTo SpreadDisplay_Error
    End If
    
    ArrayRecords = adoRs.GetRows
    adoRs.Close
    Set adoRs = Nothing
    
    
    With sPname

        .ReDraw = False
        .MaxCols = iColCnt
        Screen.MousePointer = vbHourglass

        'Title Setting
        If iColCnt <= 0 Then
            Exit Sub
        End If
        
        For iCnt = 6 To iColCnt
        
        .Col = iCnt
        .Row = 0
        .Text = "成分"
        .Row = 2
        .Text = ArrayRecords(0, iCnt - 6)
'        Call Gp_Sp_Collection(ss1, iCnt, " ", " ", " ", " ", " ", "l", pColumn1, nColumn1, mColumn1, iColumn1, aColumn1, lColumn1)
        
        Next iCnt
        
        
        .BlockMode = True
        .Row = 0
        .Col = 1
        .Row2 = -1
        .Col2 = 2
        .RowMerge = MergeAlways
        .ColMerge = MergeAlways
        .TypeHAlign = TypeHAlignCenter
        .TypeVAlign = TypeVAlignCenter
        .BlockMode = False

        .ColsFrozen = 5
        .ReDraw = True
        .Refresh

        Screen.MousePointer = vbDefault

    End With

Exit Sub

SpreadDisplay_Error:

    Set adoRs = Nothing
    ss1.ReDraw = True
    Screen.MousePointer = vbDefault

End Sub
