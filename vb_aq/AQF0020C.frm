Private Sub ss1_EditMode(ByVal Col As Long, ByVal Row As Long, ByVal Mode As Integer, ByVal ChangeMade As Boolean)
      If Gf_Sc_Authority(sAuthority, "U") Then
        Call Gp_Sp_UpdateMake(Proc_Sc("SC")("Spread"), Mode)
        Call Gp_Sp_InAuthority(Proc_Sc("Sc"), 59)
    End If
        Dim sQuery As String
    Dim iRow As Integer
    Dim v_SLAB_NO As String
    Dim v_STLGRD As String
    Dim v_CORE_SEG_YB As String
    Dim v_CORE_SEG_VAI As String
    Dim v_CORE_POR As String
    Dim v_HARF_CRACK As String
    Dim v_CORNER_CRACK As String
    Dim v_LRS As String
    
    If ss1.ActiveCol < 5 Or ss1.ActiveCol > 10 Then
      Exit Sub
    End If
    
    
    iRow = ss1.ActiveRow
    
    With ss1
        .Row = .ActiveRow
        .Col = 1
        v_SLAB_NO = .Text
        .Col = 13
        v_STLGRD = .Text
        .Col = 5
        v_CORE_SEG_YB = .Text
        .Col = 6
        v_CORE_SEG_VAI = .Text
        .Col = 7
        v_CORE_POR = .Text
        .Col = 8
        v_HARF_CRACK = .Text
        .Col = 9
        v_CORNER_CRACK = .Text
        .Col = 10
        v_LRS = .Text
    End With
    
    Set AdoRs = New adodb.Recordset

    sQuery = "select AQC1571F('" + v_SLAB_NO + "','" + v_STLGRD + "','" + v_CORE_SEG_YB + "','" + v_CORE_SEG_VAI + "','" + v_CORE_POR + "','" + v_HARF_CRACK + "','" + v_CORNER_CRACK + "','" + v_LRS + "') from dual"
    AdoRs.Open sQuery, M_CN1, adOpenKeyset
    Set AdoRs = M_CN1.Execute(sQuery)
    
    If Not AdoRs.EOF And Not AdoRs.BOF Then
      ss1.Row = ss1.ActiveRow
      ss1.Col = 11
      ss1.Text = AdoRs.Fields(0).Value
    End If
    
    AdoRs.Close
    
End Sub


