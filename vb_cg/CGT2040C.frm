Public Sub Form_Ref()
    
    Dim sMesg As String
    Dim iCount As Integer
    Dim iCol As Integer
    Dim sCurDate As String
    Dim sDel_To_Date As String
    Dim sBackColor As Variant
    
    Dim sTotnum As Double
    Dim sTotwgt As Double
    Dim s1num As Double
    Dim s1wgt As Double
    Dim s2num As Double
    Dim s2wgt As Double
    Dim s3num As Double
    Dim s3wgt As Double
    Dim s4num As Double
    Dim s4wgt As Double
    Dim s5num As Double
    Dim s5wgt As Double
    Dim s7num As Double
    Dim s7wgt As Double
    Dim sord_cnt As Integer
    Dim simpcont As String
        
    Dim sWgt As Double
    
    sCurDate = Format(Now, "YYYYMM")
    
    If Gf_Sp_ProceExist(Proc_Sc("Sc").Item("Spread")) Then Exit Sub
    
    If Val(SDT_PROD_DATE_FROM.RawData) - Val(SDT_PROD_DATE_TO.RawData) > 0 Then
         sMesg = " 时间范围输入错误，请重新输入时间信息 ！！！"
         Call Gp_MsgBoxDisplay(sMesg)
         Exit Sub
    End If
        
    If Gf_Sp_Refer(M_CN1, sc1, Mc1, Mc1("nControl")) Then
        ss1.OperationMode = OperationModeNormal
        Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
    End If
    
    With ss1
    
        If .MaxRows < 1 Then
           Exit Sub
        End If
        
        For iCount = 1 To .MaxRows
            .ROW = iCount:            .Col = SPD_WGT:        sWgt = Val(.Text):    .Col = SPD_SURF_GRD
            sTotnum = sTotnum + 1:    sTotwgt = sTotwgt + sWgt
            If .Text = "正品" Then
                s1num = s1num + 1:    s1wgt = s1wgt + sWgt
            End If
            If .Text = "改判" Then
                s2num = s2num + 1:    s2wgt = s2wgt + sWgt
            End If
            If .Text = "协议" Then
                s3num = s3num + 1:    s3wgt = s3wgt + sWgt
            End If
            If .Text = "待判" Then
                s4num = s4num + 1:    s4wgt = s4wgt + sWgt
            End If
            If .Text = "次品" Then
                s5num = s5num + 1:    s5wgt = s5wgt + sWgt
            End If
            If .Text = "废品" Then
                s7num = s7num + 1:    s7wgt = s7wgt + sWgt
            End If
            .ROW = iCount:            .Col = SPD_ORD_CNT:        sord_cnt = Val(.Text)
            If sord_cnt > 1 Then
                 Call Gp_Sp_BlockColor(ss1, 1, .MaxCols, iCount, iCount, , SSP1.BackColor)
            End If
            .ROW = iCount:            .Col = SPD_DEL_TO_DATE
            sDel_To_Date = Mid(.Value, 1, 6)
            If sDel_To_Date < sCurDate Then
                 If sord_cnt > 1 Then
                    sBackColor = SSP1.BackColor
                Else
                    sBackColor = vbWhite
                End If
                 Call Gp_Sp_BlockColor(ss1, 1, .MaxCols, iCount, iCount, &HFF&, sBackColor)
            End If
            
             '紧急订单绿色标记 2012-08-16  by  LiQian
            .ROW = iCount:       .Col = SPD_URGNT_FL
            If ss1.Text = "Y" Then
                 Call Gp_Sp_BlockColor(ss1, SPD_SLAB_NO, SPD_SLAB_NO, .ROW, .ROW, &HC000&)
                 Call Gp_Sp_BlockColor(ss1, SPD_ORD_NO, SPD_ORD_NO, .ROW, .ROW, &HC000&)
                 Call Gp_Sp_BlockColor(ss1, SPD_URGNT_FL, SPD_URGNT_FL, .ROW, .ROW, &HC000&)
            End If
            
            .ROW = iCount:
            .Col = SS1_IMP_CONT:    simpcont = Trim(.Text)
            If simpcont = "Y" Then
              Call Gp_Sp_BlockColor(ss1, SPD_SLAB_NO, SPD_SLAB_NO, iCount, iCount, SSP4.BackColor)
              Call Gp_Sp_BlockColor(ss1, SS1_IMP_CONT, SS1_IMP_CONT, iCount, iCount, SSP4.BackColor)
            End If
        Next iCount
        
    End With
    
    TXT_TOT_NUM = sTotnum:      TXT_TOT_WGT = sTotwgt
    TXT_1_NUM = s1num:          TXT_1_WGT = s1wgt
    TXT_2_NUM = s2num:          TXT_2_WGT = s2wgt
    TXT_3_NUM = s3num:          TXT_3_WGT = s3wgt
    TXT_4_NUM = s4num:          TXT_4_WGT = s4wgt
    TXT_5_NUM = s5num:          TXT_5_WGT = s5wgt
    TXT_7_NUM = s7num:          TXT_7_WGT = s7wgt
    
End Sub
