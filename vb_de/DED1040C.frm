Private Sub ss1_Click(ByVal Col As Long, ByVal Row As Long)

    If Col < 3 Then
       ss1.Row = Row
       ss1.Col = 0
       If ss1.Text = "Update" Then
            ss1.Col = SS1_PLAN_DATE:        ss1.Text = ""
            ss1.Col = 0:                    ss1.Text = ""
       Else
            ss1.Col = SS1_PLAN_DATE:        ss1.Text = txt_plan_date.Text
            ss1.Col = 0:                    ss1.Text = "Update"
       End If
    End If
    
End Sub

