Public Sub Form_Ref()
        If Trim(txt_from_DATE.RawData) = "" Or Trim(txt_to_DATE.RawData) = "" Then
            MsgBox "查询日期未输入!", vbCritical, "系统提示信息"
            Exit Sub
         End If
         
         If DateDiff("D", txt_from_DATE, txt_to_DATE) > 30 Then
            MsgBox "只能查询30天内信息!时间间隔：" & DateDiff("D", txt_from_DATE, txt_to_DATE), vbCritical, "系统提示信息"
            Exit Sub
         End If
        
            
        If Gf_Sp_Refer(M_CN1, Proc_Sc("Sc"), Mc1, Mc1("nControl"), Mc1("mControl"), True) Then
            Call MDIMain.FormMenuSetting(Me, FormType, "RE", sAuthority)
            'Call Gp_Ms_ControlLock(Mc1("pControl"), True)
'            Call Gp_Sp_EvenRowBackcolor(Proc_Sc("Sc")("Spread"))
        End If
    
End Sub

